                   assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data">

  <class name="ComplianceTransaction" table="vComplianceTransactions" dynamic-update="true">
    <id name="rowguid"/>
    <property name="TransactionId"/>
    <property name="CustomerId" />
    <property name="DTTerminalCreate" />
    <property name="TransactionType"/>
    <property name="Amount"/>
    <property name="Fee"/>
    <property name="State"/>
    <property name="ProviderId"/>
