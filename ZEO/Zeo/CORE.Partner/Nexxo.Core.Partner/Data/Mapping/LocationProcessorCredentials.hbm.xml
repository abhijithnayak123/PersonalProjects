<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data">
  <class name="LocationProcessorCredentials" table="tLocationProcessorCredentials">
    <id name="rowguid" column="LocationProcessorCredentialsPK">
      <generator class="guid"/>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="LocationProcessorCredentialsID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <many-to-one name='Location' class='MGI.Core.Partner.Data.Location' column='LocationPK'/>
    <property name="ProviderId"/>
    <property name="UserName"/>
    <property name="Password"/>
    <property name="Identifier"/>
    <property name="DTServerCreate" not-null="true" update="false"/>
    <property name="DTServerLastModified" not-null="false" insert="false"/>
		<property name="DTTerminalCreate" column="DTTerminalCreate" not-null="true" update="false"/>
		<property name="DTTerminalLastModified" column="DTTerminalLastModified" not-null="false"/>
  </class>
</hibernate-mapping>