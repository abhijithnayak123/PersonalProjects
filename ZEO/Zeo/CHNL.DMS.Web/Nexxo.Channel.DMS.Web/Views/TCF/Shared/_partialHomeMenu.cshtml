@model TCF.Channel.Zeo.Web.Models.BaseModel
@{
    object str = TempData["TempPassword"];
    bool isAdminRole = ((Convert.ToInt32(HttpContext.Current.Session["UserRoleId"]) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.SystemAdmin) || (Convert.ToInt32(HttpContext.Current.Session["UserRoleId"]) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.Tech));
    string linkDisableClass = string.Empty;
    string menuLeft = string.Empty;

    bool isTerminalSetupMenu = false;
    bool displayBlockedCountriesMenu = Convert.ToBoolean(System.Configuration.ConfigurationManager.AppSettings["DisplayBlockedCountriesMenu"]);
    if (Session["IsTerminalSetup"] != null)
    {
        isTerminalSetupMenu = Session["IsTerminalSetup"].ToString() == "1" ? true : false;
    }

    if (!isTerminalSetupMenu && !isAdminRole)
    {
        linkDisableClass = "disableLink";
        menuLeft = "nav_item_over";
    }
    else
    {
        menuLeft = "nav_item";
    }
}
<ul class="menuClass" id="selection">
    <li class="nav_head nav_icon_home">
        @Html.ActionLink("Home", "CustomerSearch", "CustomerSearch", null, new { @class = linkDisableClass })
    </li>
    @if (Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.Teller || Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.SystemAdmin || Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.Manager || (Convert.ToInt32((Session["UserRoleId"])) != (int)TCF.Zeo.Common.Util.Helper.UserRoles.ComplianceManager && Convert.ToInt32((Session["UserRoleId"])) != (int)TCF.Zeo.Common.Util.Helper.UserRoles.Tech))
    {
        if (Session["activeButton"] != null && Session["activeButton"] == "newcustomer")
        {
            <li class="nav_item_select nav_icon_customer">
                @Html.ActionLink("New Customer", "NewCustomer", "CustomerRegistration", new { }, new { @class = "nav_name " + linkDisableClass })
            </li>
        }
        else
        {

            <li class="nav_item nav_icon_customer_dis">
                @Html.ActionLink("New Customer", "NewCustomer", "CustomerRegistration", new { }, new { @class = "nav_name " + linkDisableClass })
            </li>

        }
    }
    @if (Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.SystemAdmin || Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.Tech)
    {
        if (Session["activeButton"] != null && Session["activeButton"] == "Locations")
        {
            <li class="nav_item_select nav_icon_location">
                @Html.ActionLink("Location", "EnterNewLocation", "Locations", new { }, new { @class = "nav_name " + linkDisableClass })
            </li>
        }
        else
        {
            <li class="nav_item nav_icon_location_dis">
                @Html.ActionLink("Location", "EnterNewLocation", "Locations", new { }, new { @class = "nav_name " + linkDisableClass })
            </li>
        }
    }
    @if (Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.SystemAdmin || Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.Manager || Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.Tech)
    {
        <li id="hardwareNthl" class="nav_item nav_icon_hardware_dis">
            <a id="hardware_txt"
               href="#" class="nav_name">Hardware</a>
            <ul class="sf-menu sf-vertical sub_nav">
                <li id="liTerminal" class="nav_item nav_icon_terminal_dis">
                    @Html.ActionLink("Terminal", "TerminalSetup", "TerminalSetup", new { }, new { @class = "submenuNames" })
                </li>
                <li id="liPeripheralServer" class="nav_item nav_icon_pserver_dis">
                    @Html.ActionLink("Peripheral Server", "ShowDownloaderOption", "Download", new { }, new { @class = "submenuNames" })
                </li>
                <li id="liDiagnostics" class="nav_item nav_icon_diagnostics_dis">
                    @Html.ActionLink("System Status", "DiagnosticInfo", "DiagnosticInfo", new { }, new { @class = "submenuNames" })
                </li>
                <li id="liDetailedDiagnostics" class="nav_item nav_icon_diagnostics_dis">
                    @Html.ActionLink("Epson Diagnostics", "EpsonDiagnostics", "TerminalSetup", new { }, new { @class = "submenuNames" })
                </li>
                <li id="liDetailedDiagnostics" class="nav_item nav_icon_diagnostics_dis">
                    @Html.ActionLink("Receipt Print Test", "ReceiptPrintTest", "TerminalSetup", new { }, new { @class = "submenuNames" })
                </li>
                <li id="liDetailedDiagnostics" class="nav_item nav_icon_diagnostics_dis">
                    @Html.ActionLink("Scan Check Test", "ScanCheckTest", "TerminalSetup", new { }, new { @class = "submenuNames" })
                </li>
                <li id="liDetailedDiagnostics" class="nav_item nav_icon_diagnostics_dis">
                    @Html.ActionLink("Money Order Print Test", "MoneyOrderTest", "TerminalSetup", new { }, new { @class = "submenuNames" })
                </li>
            </ul>
        </li>
    }

    @if ((Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.SystemAdmin) || (Convert.ToInt32((Session["UserRoleId"])) == (int)TCF.Zeo.Common.Util.Helper.UserRoles.Tech))
    {
        <li id="maintenance" class="nav_item nav_icon_maintenence">
            <a id="maintenance_txt"
               href="#" class="nav_name">Maintenance</a>
            <ul class="sf-menu sf-vertical sub_nav">                
                <li class="nav_item promotions_menu">
                    @Html.ActionLink("Promotions", "GetPromotion", "Promotion", new { }, new { @class = "submenuNames " })
                </li>
                @if (displayBlockedCountriesMenu)
                {
                    <li class="nav_item Blocked_Countries_menu">
                        @Html.ActionLink("Blocked Countries", "GetWUCountriesList", "WesternUnionBanner", new { }, new { @class = "submenuNames " })
                    </li>
                }      
                <li class="nav_item Features_menu">
                    @Html.ActionLink("Enable/Disable Features", "GetFeatures", "Product", new { }, new { @class = "submenuNames " })
                </li>          
            </ul>
        </li>
    }

    @if (Session["activeButton"] != null && Session["activeButton"] == "transhistory")
    {
        <li class="nav_item_select nav_icon_transhistory">
            @Html.ActionLink(@TCF.Channel.Zeo.Web.App_GlobalResources.Nexxo.TransactionHistory, "GetAgentTransactionHistory", "TransactionHistory", new { }, new { @class = "nav_name " + linkDisableClass })
        </li>
    }
    else
    {
        <li class="nav_item nav_icon_transhistory_dis">
            @Html.ActionLink(@TCF.Channel.Zeo.Web.App_GlobalResources.Nexxo.TransactionHistory, "GetAgentTransactionHistory", "TransactionHistory", new { }, new { @class = "nav_name " + linkDisableClass })
        </li>
    }
</ul>
