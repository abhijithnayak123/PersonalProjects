        isTerminalSetup = Convert.ToInt32(Session["IsTerminalSetup"]);
    }

    if (Session["IsPeripheralServerSetUp"] != null)
    {
        isPeripheralServerSetUp = Convert.ToInt32(Session["IsPeripheralServerSetUp"]);
    }
    string isAgentSessionId = string.Empty;
    if (agentSession.SessionId != null)
    {
        isAgentSessionId = agentSession.SessionId;
    }
}
