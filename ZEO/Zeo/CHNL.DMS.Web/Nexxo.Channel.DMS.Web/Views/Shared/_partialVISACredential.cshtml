@model TCF.Channel.Zeo.Web.Models.ProcessorCrendential

@Html.HiddenFor(m => m.LocationID)
@Html.HiddenFor(m => m.ProviderID)
@Html.HiddenFor(m => m.ProcessorID)

<div class="row">
    <div>
        <label class="label_txt">@Html.RequiredField(@TCF.Channel.Zeo.Web.App_GlobalResources.Nexxo.SubClientNodeId)</label>
        @Html.TextBoxFor(m => m.Identifier, new { @class = "input_box", maxLength = "15", id = "txtdpsId", tabindex = 1 })
    </div>
    <div class="error_align_left align_left_processor">
        @Html.ValidationMessageFor(m => m.Identifier)
    </div>
</div>
<div class="row">
    <div>
        <label class="label_txt">@Html.RequiredField(@TCF.Channel.Zeo.Web.App_GlobalResources.Nexxo.NodeId)</label>
        @Html.TextBoxFor(m => m.Identifier2, new { @class = "input_box", maxLength = "15", id = "txtNodeId", tabindex = 1 })
    </div>
    <div class="error_align_left align_left_processor">
        @Html.ValidationMessageFor(m => m.Identifier2)
    </div>
</div>
<div class="align_left_tabs">
    <input type="button" value="Edit" class="EditCredential anc_link_btn yellow_btn add_rec" name="Edit" id="EditVISA" tabindex="1" />
    <input type="button" style="background-color: rgb(0, 105, 55)" value="Save" class="SaveCredential" name="Save" id="SaveVISA" tabindex="1" />
</div>

