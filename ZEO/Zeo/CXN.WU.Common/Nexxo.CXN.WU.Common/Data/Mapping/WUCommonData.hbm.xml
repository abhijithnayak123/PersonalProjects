    <class name="WUCredential" table="tWUnion_Credential" lazy="true" >
    <id name="rowguid" column="WUCredentialPK">
      <generator class="guid" />
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="WUCredentialID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <property name="WUServiceUrl">
      <column name="WUServiceUrl" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="WUClientCertificateSubjectName">
      <column name="WUClientCertificateSubjectName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="AccountIdentifier">
      <column name="AccountIdentifier" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CounterId">
      <column name="CounterId" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ChannelName">
      <column name="ChannelName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ChannelVersion">
      <column name="ChannelVersion" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ChannelPartnerId">
      <column name="ChannelPartnerId" sql-type="bigint" not-null="false" />
    </property>
    <property name="DTServerCreate">
      <column name="DTServerCreate" sql-type="datetime" not-null="true" />
    </property>
    <property name="DTServerLastModified">
      <column name="DTServerLastModified" sql-type="datetime" not-null="false" />
    </property>
  </class>
