<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="MGI.Cxn.Check.Chexar"
    namespace="MGI.Cxn.Check.Chexar.Data">

  <class name="ChexarTrx" table="tChxr_Trx" dynamic-update="true" >
    <id name="rowguid" column="ChxrTrxPK">
      <generator class="guid">
      </generator>
    </id>
    <property name="Id" column="ChxrTrxID"/>

    <property name="Amount" />
    <property name="ChexarAmount" />
    <property name="ChexarFee" />
    <property name="CheckDate" />
    <property name="CheckNumber" />
    <property name="RoutingNumber" />
    <property name="AccountNumber" />

    <property name="Micr" />
    <property name="Latitude" />
    <property name="Longitude" />
    <property name="InvoiceId" />
    <property name="TicketId" />
    <property name="WaitTime" />
    <property name="Status" />
    <property name="ChexarStatus" />
    <property name="SubmitType" />
    <property name="ReturnType" />
    <property name="DeclineCode" />
    <property name="Message" />
    <property name="Location" />

    <many-to-one name="Account" class="MGI.Cxn.Check.Chexar.Data.ChexarAccount"
                 column="ChxrAccountPK" insert="true" update="false" cascade="none" not-null="true" />

    <property name="DTTerminalCreate" update="false"/>
    <property name="DTTerminalLastModified" not-null="false"/>
    <property name="ChannelPartnerID" />
    <property name="DTServerCreate" update="false"/>
    <property name="DTServerLastModified" not-null="false"/>
	  <property name="IsCheckFranked" not-null="true"/>
  </class>
</hibernate-mapping>