<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="MGI.Cxn.Check.Chexar"
    namespace="MGI.Cxn.Check.Chexar.Data">

	<class name="ChexarAccount" table="tChxr_Account" >
		<id name="rowguid" column="ChxrAccountPK">
			<generator class="guid">
			</generator>
		</id>
		<property name="Id" generated="insert" update="false" insert="false">
			<column name="ChxrAccountID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
		</property>		
		<property name="Badge" />
		<property name="FirstName" />
		<property name="LastName" />
		<property name="ITIN" />
		<property name="SSN" />

		<property name="DateOfBirth" />
		<property name="Address1" />
		<property name="Address2" />
		<property name="City" />
		<property name="State" />
		<property name="Zip" />
		<property name="Phone" />
		<property name="Occupation" />
		<property name="Employer" />
		<property name="EmployerPhone" />
		<property name="GovernmentId">
			<column name="IDCardNumber" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="IDCardType" />
		<property name="IDCardIssuedCountry" />
		<property name="IDCardIssuedDate" />
		<property name="IDCardImage" />
		<property name="IDCardExpireDate" />
		<property name="CardNumber" />
		<property name="CustomerScore" />

		<property name="DTServerCreate" update="false"/>
		<property name="DTServerLastModified" insert="false" not-null="false"/>
		<property name="DTTerminalCreate" update="false"/>
		<property name="DTTerminalLastModified" insert="false" not-null="false"/>
		<property name="IDCode"/>
	</class>
</hibernate-mapping>
