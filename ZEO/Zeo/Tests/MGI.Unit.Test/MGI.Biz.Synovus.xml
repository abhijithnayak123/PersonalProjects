	<object id ="InterceptedClientCustomerService" type="MGI.Biz.Synovus.Impl.SynovusCustomer,MGI.Biz.Synovus">
		<!--<property name="DBProvider" ref="Cxn.Synovus.DbProvider"/>-->
		<property name ="CxnClientCustomerService" ref="TCISGateway"/>
		<property name ="CxnFISCustomer" ref="FISConnect"/>
		<property name="NLogger" ref="NLoggerRef"></property>
	</object>
	
	<object id="SynovusCustomerEditEventListener" type="MGI.Biz.Synovus.Impl.SynovusCustomerEditEventListener,MGI.Biz.Synovus">
		<property name ="CXNClientCustomerSvc" ref="TCISGateway"/>
		<property name ="CXNCustomerSvc" ref="FISConnect"/>
		<property name="ClientCustomerSvc" ref ="InterceptedClientCustomerService"/>
		<property name ="FISAccountRepo" ref="FISAccountRepo"/>
		<property name ="BIZFundsEngineSvc" ref="InterceptedBIZFundsEngine"/>
		<property name="PTNRCustomerService" ref="Core.Partner.CustomerService"/>
		<property name="CXECustomerService" ref="CXECustomerService"/>
	</object>


	<object id="SynovusCustomerRegistrationEventListener" type="MGI.Biz.Synovus.Impl.SynovusCustomerRegistrationEventListener,MGI.Biz.Synovus">
		<property name ="CXNClientCustomerSvc" ref="TCISGateway"/>
		<property name ="CXNCustomerSvc" ref="FISConnect"/>
		<property name="ClientCustomerSvc" ref ="InterceptedClientCustomerService"/>
		<property name ="FISAccountRepo" ref="FISAccountRepo"/>
		<property name ="BIZFundsEngineSvc" ref="InterceptedBIZFundsEngine"/>
		<property name="PartnerCustomerService" ref="Core.Partner.CustomerService"/>
		<property name="CXECustomerService" ref="CXECustomerService"/>
	</object>

	<object id="SynovusGPRAddEventListener" type="MGI.Biz.Synovus.Impl.SynovusGprAddEventListener,MGI.Biz.Synovus">
		<property name ="CXNClientCustomerSvc" ref="TCISGateway"/>
		<property name ="CXNClientFISRepo" ref="FISAccountRepo"/>
		<property name="NLogger" ref="NLoggerRef"></property>
	</object>
	
	<object id="FISConnect" factory-object="MockClientCustomerServiceSynovus" factory-method="CreateFISCustomer" ></object>
	
	<object id="FISAccountRepo" factory-object="MockClientCustomerServiceSynovus" factory-method="CreateIntsanceOfFISAccount" ></object>

</objects>
