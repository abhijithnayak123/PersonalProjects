<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MGI.Cxn.Customer.FIS" namespace="MGI.Cxn.Customer.FIS.Data" xmlns="urn:nhibernate-mapping-2.2">
  <class name="FISAccount" table="tFIS_Account">
    <id name="rowguid" column="FISAccountPK">
      <generator class="guid"/>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="FISAccountID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <property name="FirstName" />
    <property name="MiddleName" />
    <property name="LastName" />
    <property name="LastName2" />
    <property name="MothersMaidenName" />
	<property name="DateOfBirth">
	  <column name="DOB" sql-type="datetime" not-null="false" />
	</property>
    <property name="Address1" />
    <property name="Address2" />
    <property name="City" />
    <property name="State" />
    <property name="ZipCode" />
    <property name="Phone1" />
    <property name="Phone2" />
    <property name="SSN" />
    <property name="Gender"/>
    <property name="PartnerAccountNumber"/>
    <property name="RelationshipAccountNumber"/>
    <property name="ProfileStatus"/>
    <property name="DTTerminalCreate"/>
    <property name="DTTerminalLastModified"/>
    <property name="BankId"/>
    <property name="BranchId"/>
    <property name="DTServerCreate"/>
    <property name="DTServerLastModified"/>
	<property name="IDCode" />

	<property name="IsCISSuccess" />
	<property name="IsCNECTSuccess" />
	<property name="IsPREPDSuccess" />
	</class>
</hibernate-mapping>