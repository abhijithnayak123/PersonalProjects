<?xml version="1.0" encoding="utf-8" ?>

<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns:db="http://www.springframework.net/database"
		 xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

	<import resource="assembly://MGI.Cxn.Customer.FIS/MGI.Cxn.Customer.FIS.Impl/Cxn.Customer.FIS.Db.Spring.xml" />

	<object id="FISCredentialRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Customer.FIS.Data.FISCredential&gt;, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="Cxn.Customer.FIS.SessionFactory"></property>
	</object>

	<object id="FISAccountRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Customer.FIS.Data.FISAccount&gt;, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="Cxn.Customer.FIS.SessionFactory"></property>
	</object>

	<object id="FISErrorRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Customer.FIS.Data.FISError&gt;, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="Cxn.Customer.FIS.SessionFactory"></property>
	</object>

	<object id="FISIO" type="MGI.Cxn.Customer.FIS.Impl.FISIOImpl, MGI.Cxn.Customer.FIS">
		<property name="FISCredentialRepo" ref="FISCredentialRepo"/>
		<property name="FISErrorRepo" ref="FISErrorRepo"/>
    <property name="NLogger" ref="NLoggerRef"></property>
	</object>

	<object id="FISGateway" type="MGI.Cxn.Customer.FIS.Impl.FISGateway, MGI.Cxn.Customer.FIS">
		<property name="FisIO" ref="FISIO"/>
		<property name="FISAccountRepo" ref ="FISAccountRepo"/>
		<property name="EnvironmentIndicator" value="${EnvironmentIndicator}"/>
    <property name="NLogger" ref="NLoggerRef"></property>
	</object>

	<!-- Added for SQL Injection US#1789 -->

	<object id="FISConnect" type="MGI.Cxn.Customer.FIS.Impl.FISConnect, MGI.Cxn.Customer.FIS">
		<property name="FISConnectRepo" ref="FISConnectRepo"/>
    <property name="NLogger" ref="NLoggerRef"></property>
	</object>

	<object id="FISConnectRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Customer.FIS.Data.FISConnect&gt;, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="Cxn.Customer.FIS.SessionFactory"></property>
	</object>
	<!-- Added for SQL Injection Complete US#1789 -->


</objects>
