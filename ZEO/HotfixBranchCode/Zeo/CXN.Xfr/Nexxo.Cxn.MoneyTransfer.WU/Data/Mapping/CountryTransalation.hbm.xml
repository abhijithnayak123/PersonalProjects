<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MGI.Cxn.MoneyTransfer.WU" namespace="MGI.Cxn.MoneyTransfer.WU.Data" xmlns="urn:nhibernate-mapping-2.2">
  <class name="CountryTransalation" table="tWunion_CountryTranslation" lazy="true" >
    <id name="rowguid" column="WUCountryTranslationPK">
      <generator class="guid" />
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="WUCountryID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <property name="CountryCode">
      <column name="ISOCountryCode" sql-type="varchar" not-null="false" />
    </property>
    <property name="Name">
      <column name="TranslationName" sql-type="varchar" not-null="false" />
    </property>
    <property name="Language">
      <column name="LanguageCode" sql-type="varchar" not-null="false" />
    </property>
    <property name="DTServerCreate">
      <column name="DTServerCreate" sql-type="datetime" not-null="true" />
    </property>
    <property name="DTServerLastModified">
      <column name="DTServerLastModified" sql-type="datetime" not-null="false" />
    </property>
  </class>
</hibernate-mapping>

