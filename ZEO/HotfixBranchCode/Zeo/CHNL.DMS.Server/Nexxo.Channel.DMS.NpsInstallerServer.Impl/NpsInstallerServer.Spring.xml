<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns:db="http://www.springframework.net/database"
		 xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">
	<!-- These are Channel Partner object definitions... 
    These should work in any environment and only change when the Partner services wiring needs a change.
  -->
	<import resource="assembly://MGI.Channel.DMS.NpsInstallerServer.Impl/MGI.Channel.DMS.NpsInstallerServer.Impl/NpsInstallerServer.Db.Spring.xml" />

	<!-- Channel Partner Service -->
	<object id="ChannelPartnerServiceRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Channel.DMS.NpsInstallerServer.Data.ChannelPartner&gt;, MGI.Common.DataAccess" autowire="byType" >
		<property name="SessionFactory" ref="SessionFactoryCorePartner"></property>
	</object>
	<!-- End Of Channel Partner Service -->

  <!-- Location Service -->
	<object id="LocationRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Channel.DMS.NpsInstallerServer.Data.Location&gt;, MGI.Common.DataAccess"  >
		<property name="SessionFactory" ref="SessionFactoryCorePartner"></property>
	</object>
	<!-- End of Location Service -->

	<!-- Terminal Service -->
	<!--<object id="NHibTerminalRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Channel.DMS.NpsInstallerServer.Data.Terminal>, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="SessionFactoryCorePartner"/>
	</object>-->
	<!-- End of Terminal Service -->

  <!-- NpsTerminal Service -->
	<object id="NpsTerminalRepository" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Channel.DMS.NpsInstallerServer.Data.NpsTerminal&gt;, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="SessionFactoryCorePartner"/>
	</object>
