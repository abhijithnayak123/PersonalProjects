<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns:db="http://www.springframework.net/database"
		 xmlns:tx="http://www.springframework.net/tx"
		 xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

	<import resource="assembly://MGI.Cxn.Fund.FirstView/MGI.Cxn.Fund.FirstView/Cxn.Fund.FirstView.Db.Spring.xml"/>

	<object id="FirstViewGateway" type="MGI.Cxn.Fund.FirstView.Impl.FVGatewayActivate, MGI.Cxn.Fund.FirstView">
		<property name="FundProcessorDAL" ref="FundProcessorDAL" />
		<property name="DataProtection" ref="CXNFundsDataProtection${DataProtectionService}"/>
	</object>

	<object id="FundProcessorDAL" type="MGI.Cxn.Fund.FirstView.Impl.FundsProcessorImpl, MGI.Cxn.Fund.FirstView">
		<property name="CredentialRepository" ref="CredentialRepository" />
		<property name="TransactionMappingRepository" ref="TransactionMappingRepository" />
		<property name="GPRCardRepository" ref="GPRCardRepository"/>
		<property name="IdTypeRepository" ref="IdTypeRepository"/>
		<property name="DataProtectionSvc" ref="CXNFundsDataProtection${DataProtectionService}"/>
	</object>

	<object id="TransactionMappingRepository" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Fund.FirstView.Data.FirstViewTransaction&gt;, MGI.Common.DataAccess" >
		<property name="SessionFactory" ref="Cxn.Fund.FirstView.SessionFactory"></property>
	</object>

	<object id="CredentialRepository" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Fund.FirstView.Data.FirstViewCredentials&gt;, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="Cxn.Fund.FirstView.SessionFactory"></property>
	</object>

	<object id="GPRCardRepository" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Fund.FirstView.Data.FirstViewCard&gt;, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="Cxn.Fund.FirstView.SessionFactory"></property>
	</object>

	<object id="IdTypeRepository" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Fund.FirstView.Data.FirstViewIdTypes&gt;, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="Cxn.Fund.FirstView.SessionFactory"></property>
	</object>

	<object id="CXNFundsDataProtectionSimulator" type="MGI.Common.DataProtection.Impl.DataProtectionSimulator, MGI.Common.DataProtection"/>
	<object id="CXNFundsDataProtectionService" type="MGI.Common.DataProtection.Impl.DataProtectionService, MGI.Common.DataProtection"/>
</objects>
