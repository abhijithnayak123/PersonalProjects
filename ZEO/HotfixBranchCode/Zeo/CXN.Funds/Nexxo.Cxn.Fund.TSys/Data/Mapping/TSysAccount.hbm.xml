<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MGI.Cxn.Fund.TSys" namespace="MGI.Cxn.Fund.TSys.Data" xmlns="urn:nhibernate-mapping-2.2">
  <class name="TSysAccount" table="tTSys_Account">
    <id name="rowguid" column="TSysAccountPK">
      <generator class="guid"/>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="TSysAccountID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <property name="ProgramId"/>
    <property name="ExternalKey"/>
    <property name="CardNumber"/>
    <property name="AccountId"/>
    <property name ="UserId"/>
    <property name="FirstName"/>
    <property name="MiddleName"/>
    <property name="LastName"/>
	  <property name="DateOfBirth">
		  <column name="DOB" sql-type="datetime" not-null="false" />
	  </property>
    <property name="SSN"/>
    <property name="Address1"/>
    <property name="Address2"/>
    <property name="City"/>
    <property name="State"/>
    <property name="ZipCode"/>
    <property name="Country"/>
    <property name="Phone"/>
    <property name="PhoneType"/>
    <property name="Activated"/>
    <property name="FraudScore"/>
    <property name="FraudResolution"/>
    <property name="DTTerminalCreate"/>
    <property name="DTTerminalLastModified"/>
    <property name="DTServerCreate"/>
    <property name="DTServerLastModified"/>
	<property name="IDCode"/>
  </class>
</hibernate-mapping>