<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="MGI.Cxn.Fund.Visa" namespace="MGI.Cxn.Fund.Visa.Data" xmlns="urn:nhibernate-mapping-2.2">
	<class name="CardClass" table="tVisa_CardClass" lazy="true" >
		<id name="rowguid" column="VisaCardClassPK">
			<generator class="guid" />
		</id>
		<property name="Id" generated="insert" update="false" insert="false">
			<column name="VisaCardClassId" sql-type="bigint IDENTITY(1000000000,1)" not-null="true" />
		</property>
		<property name="StateCode">
			<column name="StateCode" sql-type="varchar" not-null="true" unique="true" />
		</property>
		<property name="Class">
			<column name="CardClass" sql-type="int" not-null="true" />
		</property>
		<property name="DTServerCreate">
			<column name="DTServerCreate" sql-type="datetime" not-null="false" />
		</property>
		<property name="DTServerLastModified">
			<column name="DTServerLastModified" sql-type="datetime" not-null="false" />
		</property>
		<property name="ChannelPartnerId">
			<column name="ChannelPartnerId" sql-type="int" not-null="false" />
		</property>
	</class>
</hibernate-mapping>