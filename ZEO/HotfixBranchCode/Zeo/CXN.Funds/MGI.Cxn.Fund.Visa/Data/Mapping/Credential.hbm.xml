<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="MGI.Cxn.Fund.Visa" namespace="MGI.Cxn.Fund.Visa.Data" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Credential" table="tVisa_Credential" lazy="true" >
		<id name="rowguid" column="VisaCredentialPK">
			<generator class="guid" />
		</id>
		<property name="Id" generated="insert" update="false" insert="false">
			<column name="VisaCredentialID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
		</property>
		<property name="ServiceUrl">
			<column name="ServiceUrl" sql-type="varchar" not-null="true" />
		</property>
		<property name="CertificateName">
			<column name="CertificateName" sql-type="varchar" not-null="true" />
		</property>
		<property name="UserName">
			<column name="UserName" sql-type="varchar" not-null="true" />
		</property>
		<property name="Password">
			<column name="Password" sql-type="varchar" not-null="true" />
		</property>
		<property name="ClientNodeId">
			<column name="ClientNodeId" sql-type="bigint" not-null="true" />
		</property>
		<property name="CardProgramNodeId">
			<column name="CardProgramNodeId" sql-type="bigint" not-null="true" />
		</property>
		<property name="SubClientNodeId">
			<column name="SubClientNodeId" sql-type="bigint" not-null="true" />
		</property>
		<property name="StockId">
			<column name="StockId" sql-type="varchar" not-null="true" />
		</property>
		<property name="ChannelPartnerId">
			<column name="ChannelPartnerId" sql-type="bigint" not-null="true" />
		</property>		
		<property name="DTServerCreate">
			<column name="DTServerCreate" sql-type="datetime" not-null="true" />
		</property>
		<property name="DTServerLastModified">
			<column name="DTServerLastModified" sql-type="datetime" not-null="false" />
		</property>
	</class>
</hibernate-mapping>