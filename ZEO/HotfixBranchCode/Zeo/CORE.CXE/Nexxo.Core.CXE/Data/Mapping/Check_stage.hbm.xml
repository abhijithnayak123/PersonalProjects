<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.CXE"
                   namespace="MGI.Core.CXE.Data.Transactions.Stage" auto-import="false">

  <class name="Check" table="tTxn_Check_Stage" dynamic-update="true" >
    <id name="rowguid" column="CheckPK" >
      <generator class="guid"/>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="CheckID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <property name="Amount" />
    <property name="Fee" />
    <property name="IssueDate" not-null="false" />
    <property name="MICR" />
    <property name="CheckType" />
    <many-to-one name="Account" class="MGI.Core.CXE.Data.Account" column="AccountPK" insert="true" update="false" cascade="none" not-null="true" />
    <one-to-one name="Images" class="MGI.Core.CXE.Data.CheckImages" constrained="false" cascade="all-delete-orphan"/>
    <property name="Status" />
    <property name="DTTerminalCreate" update="false"/>
    <property name="DTTerminalLastModified" not-null="false"/>
    <property name="DTServerCreate" update="false"/>
    <property name="DTServerLastModified" not-null="false"/>
  </class>

</hibernate-mapping>