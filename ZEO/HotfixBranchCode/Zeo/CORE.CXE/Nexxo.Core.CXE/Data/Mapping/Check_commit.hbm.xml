<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.CXE"
                   namespace="MGI.Core.CXE.Data.Transactions.Commit" auto-import="false">

  <class name="Check" table="tTxn_Check_Commit" dynamic-update="true" >
    <id name="rowguid" column="CheckPK" />
    <property name="Id" column="CheckID" />
    <property name="Amount" />
    <property name="Fee" />
    <property name="IssueDate" not-null="false" />
    <property name="MICR" />
    <property name="CheckType" />
    <many-to-one name="Account" class="MGI.Core.CXE.Data.Account" column="AccountPK" insert="true" update="false" cascade="none" not-null="true" />
    <one-to-one name="Images" class="MGI.Core.CXE.Data.CheckImages" constrained="true" cascade="all-delete-orphan"/>
    <property name="Status" />
    <property name="DTTerminalCreate" update="false"/>
    <property name="DTTerminalLastModified" insert="false" not-null="false"/>
    <property name="DTServerCreate" update="false"/>
    <property name="DTServerLastModified" insert="false" not-null="false"/>
  </class>

</hibernate-mapping>