<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.CXE"
                   namespace="MGI.Core.CXE.Data">
  <class name="CustomerPreferedProduct" table="tCustomerPreferedProducts">
    <id name="rowguid">
      <generator class="guid"/>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="Id" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <property name="AlloyID">
      <column name="PAN" sql-type="bigint" not-null="true" />
    </property>
    <property name="ProductId">
      <column name="ProductId" sql-type="bigint" not-null="true" />
    </property>
    <property name="AccountNumber">
      <column name="AccountNumber" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="PhoneNumber">
      <column name="PhoneNumber" sql-type="bigint" not-null="false" />
    </property>
    <property name="AccountPIN">
      <column name="AccountPIN" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="AccountDOB">
      <column name="AccountDOB" sql-type="datetime" not-null="false" />
    </property>
    <property name="AccountName">
      <column name="AccountName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Enabled">
      <column name="Enabled" sql-type="bit" not-null="false" />
    </property>
    <property name="DTCreate">
      <column name="DTCreate" sql-type="datetime" not-null="false" />
    </property>
    <property name="DTLastMod">
      <column name="DTLastMod" sql-type="datetime" not-null="false" />
    </property>
    <property name="Operator">
      <column name="Operator" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="AgentId">
      <column name="AgentId" sql-type="int" not-null="false" />
    </property>
    <property name="TenantId">
      <column name="TenantId" sql-type="nvarchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>