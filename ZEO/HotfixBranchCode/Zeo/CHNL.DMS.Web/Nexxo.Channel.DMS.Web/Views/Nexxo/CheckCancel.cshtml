@model MGI.Channel.DMS.Web.Models.CheckDetails
@{
    ViewBag.Title = MGI.Channel.DMS.Web.App_GlobalResources.Nexxo.CheckCancelTitle;
}
<link href="@Url.Content("~/Content/css/Common/jquery-ui.css")" rel="stylesheet" type="text/css"/>
<script src="@Url.Content("~/Scripts/Check/Check.js")" type="text/javascript"></script>

    <div>
        <ul class="page_head product_head">
            <li>Check Cancelled</li>
        </ul>
    </div> 
    <br />

<div class="error_txt"> @ViewBag.ErrorMessage </div>
<div>
    <img alt=" " id="CheckFrontImage" src="" class="check_img" />
    <script language="javascript" type="text/javascript">
		$(document).ready(function ()
		{
		    ShowCheckFrontImage("@Model.CheckFrontImage");
		});
    </script>
</div>
<center class="status_msg">
    @using (Html.BeginForm("CheckCancel", "CheckCancel"))
    {
        <div class="field-validation-error txt_center row">
            @MGI.Channel.DMS.Web.App_GlobalResources.Nexxo.CheckCancelCancelMessage
        </div>
        
        <div>
            @Html.Partial("_CheckDetails", Model)
            <div>
                <input type="submit" id="Exit" name="Exit" class="check_cancel_btn" value="@MGI.Channel.DMS.Web.App_GlobalResources.Nexxo.CheckCancelExitButton"/>
            </div>
            
        </div>
    }
</center>