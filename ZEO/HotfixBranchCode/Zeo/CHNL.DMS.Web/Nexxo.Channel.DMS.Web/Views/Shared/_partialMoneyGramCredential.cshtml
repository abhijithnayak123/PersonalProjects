@model MGI.Channel.DMS.Web.Models.ProcessorCrendential
@Html.HiddenFor(m => m.LocationID)
@Html.HiddenFor(m => m.ProviderID)
@Html.HiddenFor(m => m.ProcessorID)
<div class="row">
	<div>
		<label class="label_txt1">@Html.RequiredField(@MGI.Channel.DMS.Web.App_GlobalResources.Nexxo.MoneyGramAgentID)</label>
		@Html.TextBoxFor(m => m.UserName, new { @class = "input_box", maxLength = "8", id = "errorMGIAgentId" })
	</div>
	<div class="error_align_left1 align_left_processor moneygram-validation-error">
		@Html.ValidationMessageFor(m => m.UserName)
	</div>
</div>
<div class="row">
	<div>
		<label class="label_txt1">@Html.RequiredField(@MGI.Channel.DMS.Web.App_GlobalResources.Nexxo.PosID)</label>
		@Html.TextBoxFor(m => m.Identifier, new { @class = "input_box", maxLength = "2", id = "errorPosId" })
	</div>
	<div class="error_align_left align_left_processor">
		@Html.ValidationMessageFor(m => m.Identifier)
	</div>
</div>
<div class="row">
	<div>
		<label class="label_txt1">@Html.RequiredField(@MGI.Channel.DMS.Web.App_GlobalResources.Nexxo.Password)</label>
		@Html.PasswordFor(m => m.Password, new { @class = "input_box", maxLength = "8", TextMode = "Password" })
	</div>
	<div class="error_align_left1 align_left_processor moneygram-validation-error">
		@Html.ValidationMessageFor(m => m.Password)
	</div>
</div>
<div class="align_left_tabs moneygram">
	<input type="button" value="Edit" class="EditCredential anc_link_btn yellow_btn add_rec" name="Edit" id="EditMoneyGram" />
	<input type="button" style="background-color: rgb(0, 105, 55)" value="Save" class="SaveCredential" name="Save" id="SaveMoneyGram" />
</div>
