@model MGI.Channel.DMS.Web.Models.ProfileSummary
<script src="@Url.Content("~/Scripts/Common/lib/jquery.maskedinput-1.3.js")" type="text/javascript"></script>
<script type="text/javascript">
    $(document).ready(function () {
        $("#PrimaryPhone").mask("999-999-9999");
        $("#EmployerPhoneNumber").mask("999-999-9999");
    });
 
 </script>
<div class="cust_prof_sum_wrap">
<div class="row">
    @Html.LabelFor(model => model.Name, new { @class = "row_label_txt row_width16"})
    @Html.TextBoxFor(model => model.Name, new { @class = "row_input_bg m_name", @disabled = "disabled", title = Model.Name })
</div>
<div class="row">
    @Html.LabelFor(model => model.Gender, new { @class = "row_label_txt row_width16"})
    @Html.TextBoxFor(model => model.Gender, new { @class = "disable_txt", @disabled = "disabled", title = Model.Gender })
</div>
<div class="row">
    @Html.LabelFor(model => model.PrimaryPhone, new { @class = "row_label_txt row_width16"})
