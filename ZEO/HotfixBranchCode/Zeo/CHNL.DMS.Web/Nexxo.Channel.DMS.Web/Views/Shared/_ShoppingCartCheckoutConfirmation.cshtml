@using (Html.BeginForm("ShoppingCartCheckout", "ShoppingCart", FormMethod.Get))
{
	<div id="dyntext">
		<p class="row_shift12"> Complete all product transactions prior to executing Prepaid load or Withdraw. </p>
        <br /><br />
		<p class="row_shift12"> Would you like to continue to checkout screen for executing Prepaid load or withdraw? </p>
	</div>
	<div class="shift25">
		<ul>
			<li class="popUpCancelBtnWidth">
				<input type="button" value="No" onclick="javascript:$('#ShowGPRtrxConfirmationMessage').dialog('destroy').remove();" style="background-color: #ad2525;" />
			</li>
			<li class="popUpConfrmBtnWidth">
				<input type="submit" value="Yes" id="btnConfirm" />
			</li>
		</ul>
	</div>
}
<script type="text/javascript">
    $(document).ready(function () {
        $('#btnConfirm').focus();
    });
</script>
