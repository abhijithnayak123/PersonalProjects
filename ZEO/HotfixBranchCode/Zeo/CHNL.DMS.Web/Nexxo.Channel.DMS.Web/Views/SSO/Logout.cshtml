@model MGI.Channel.DMS.Web.Models.SSOLogin
@{
    Layout = null;
    string isNpsLocal = System.Configuration.ConfigurationManager.AppSettings["IsNpsLocal"];
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@Model.ChannelPartnerName SSO Login</title>
    <link href="@Url.Content("~/Content/css/Common/layout-rule.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/Common/lib/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Common/BrowserCustomisation.js")" type="text/javascript"></script>
    <script type="text/javascript">
        $(document).ready(function () {            
            if (typeof (Storage) !== "undefined") {
                $("#logo_agent").attr("src", "../Content/images/" + localStorage.getItem("ChannelPartnerName") + ".png");
            }
        });
    </script>

</head>
<body>
    <div id="layout_container_wrap">
        <div id="layout_primary_head">
            <div class="logo_bg">
                @{ string logoUrl = Url.Content(string.Format("~/Content/images/{0}.png", @Model.ChannelPartnerName));
                <img class="logo_agent" id="logo_agent" alt="" />
                    if (Model != null && Model.channelPartner != null && Model.channelPartner.IsMGIAlloyLogoEnable)
                    {
						<img class="logo_nexxo" alt=" " src="~/Content/images/MGiAlloylogo.png" />
					}
				}
            </div>
            <div class="page_head">
                <div class="bread_head">
                    <a class="menu" href="#"></a>
                </div>
            </div>
        </div>
		<br />
		<br />
		<div class="ssoerrortext">
			You have been logged out.
		</div>
    </div>


</body>
</html>
