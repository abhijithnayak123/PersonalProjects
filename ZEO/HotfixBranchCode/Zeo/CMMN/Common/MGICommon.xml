<?xml version="1.0" encoding="utf-8" ?>

<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns:db="http://www.springframework.net/database"
		 xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">
  
  <object id="mi" type="MGI.Common.Util.TraceOutputMessageInspector, MGI.Common">
    <property name="NLogger" ref="NLoggerRef" />
  </object>
  
  <object id="NLoggerRef" type="MGI.Common.Util.NLoggerCommon, MGI.Common">
  </object>
  <object id="TLoggerRef" type="MGI.Common.Util.TLoggerCommon, MGI.Common">
    <property name="NLogger" ref="NLoggerRef" />
    <property name="IsTransactionLogRequired" value="${IsTransactionLogRequired}"/>
  </object>
</objects> 