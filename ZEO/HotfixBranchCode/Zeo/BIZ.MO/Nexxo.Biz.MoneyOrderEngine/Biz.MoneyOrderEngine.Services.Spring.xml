<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns:db="http://www.springframework.net/database"
		 xmlns:tx="http://www.springframework.net/tx"
		 xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

	<import resource="assembly://MGI.Core.CXE/MGI.Core.CXE/CXESpring.xml"/>
	<import resource="assembly://MGI.Core.Partner/MGI.Core.Partner/Core.Partner.Services.Spring.xml"/>
	<import resource="assembly://MGI.Biz.Compliance/MGI.Biz.Compliance/Biz.Compliance.Services.Spring.xml"/>
	<object id="ThrowsBizMoneyOrderEngineExceptionAdvice" type="MGI.Biz.MoneyOrderEngine.Impl.Advice.ThrowsBizMoneyOrderEngineExceptionAdvice, MGI.Biz.MoneyOrderEngine"/>

  <object id="InterceptedBizMoneyOrderEngine" type="MGI.Biz.MoneyOrderEngine.Impl.MoneyOrderEngineServiceImpl, MGI.Biz.MoneyOrderEngine">
    <property name="SessionSvc" ref="CustomerSessionService"/>
    <property name="CxeCustSvc" ref="CXECustomerService"/>
    <property name="AcctSvc" ref="AccountService"/>
    <property name="PTNRMoneyOrderSvc" ref="PartnerMoneyOrderService"/>
    <property name="CxeMoneyOrderSvc" ref="CXEMoneyOrderService"/>
    <property name="CustomerService" ref="Core.Partner.CustomerService"/>
    <property name="FeeSvc" ref="PartnerFeeService"/>
    <property name="NexxoIdTypeService" ref ="CoreNexxoDataStructuresService" />
    <property name="LimitService" ref="BIZLimitService" />
    <property name="ChannelPartnerSvc" ref="CorePartnerChannelPartnerService"/>
    <property name="CheckPrintRepo" ref="BIZMoneyOrderCheckPrintRepo"/>
    <property name="MoneyOrderImageSvc" ref="PtrMoneyOrderImageService"/>
    <property name="CustomerFeeAdjustmentService" ref="CoreCustomerFeeAdjustmentService"></property>
    <property name="FeeAdjustmentService" ref="FeeAdjustmentService"></property>
    <property name="AllowDuplicateMoneyOrder" value="${AllowDuplicateMoneyOrder}"></property>
	<property name="MongoDBLogger" ref="TLoggerRef"></property>
  </object>
  
	<object id="BIZMoneyOrderEngine" type="Spring.Aop.Framework.ProxyFactoryObject, Spring.Aop">
		<property name="Target" ref="InterceptedBizMoneyOrderEngine"/>
		<property name="InterceptorNames">
			<list>
				<value>ThrowsBizMoneyOrderEngineExceptionAdvice</value>
			</list>
		</property>
	</object>
	<object id="BIZMoneyOrderCheckPrintRepo" type="MGI.Biz.MoneyOrderEngine.Impl.MoneyOrderCheckPrintTemplateRepo, MGI.Biz.MoneyOrderEngine">
		<constructor-arg index="0" value= "${ReceiptServerURL}"/>
		<constructor-arg index="1" value="${ReceiptServerDBName}" />
		<constructor-arg index="2" value="${ReceiptServerBucketName}" />
		<property name="PrintTemplateRepo" ref="PrintTemplate"></property>
	</object>
	
	<object id="PrintTemplate" type="MGI.Biz.Common.Impl.PrintTemplate, MGI.Biz.Common">
		<property name="NLogger" ref="NLoggerRef"></property>
		<property name="MongoDBLogger" ref="TLoggerRef"></property>
	</object>
	
</objects>
