<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Compliance"
                   namespace="MGI.Core.Compliance.Data">
	<class name="ComplianceProgram" table="tCompliancePrograms">
		<id name="rowguid" column="ComplianceProgramPK">
			<generator class="guid"/>
		</id>
		<property name="Id" generated="insert" update="false" insert="false">
			<column name="ComplianceProgramID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
		</property>
		<property name="Name"/>
		<property name="RunOFAC"/>

		<bag name="LimitTypes" cascade="all-delete-orphan" lazy="false">
			<key column="ComplianceProgramPK"/>
			<one-to-many class="MGI.Core.Compliance.Data.LimitType"/>
		</bag>

		<property name="DTServerCreate" column="DTServerCreate" update="false"/>
		<property name="DTServerLastModified" column="DTServerLastModified" not-null="false"/>
	</class>
</hibernate-mapping>
