<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data">
  <class name="NotificationHost" table="tNotificationHosts" lazy="true" >
    <id name="Id">
      <column name="NotificationHostIdPK" sql-type="int" not-null="true" />
    </id>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="MailGateway">
      <column name="MailGateway" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="MaxMessageLen">
      <column name="MaxMessageLen" sql-type="int" not-null="false" />
    </property>
    <property name="DTServerCreate">
      <column name="DTServerCreate" sql-type="datetime" not-null="false" />
    </property>
    <property name="DTServerLastModified">
      <column name="DTServerLastModified" sql-type="datetime" not-null="false" />
    </property>
  </class>
</hibernate-mapping>