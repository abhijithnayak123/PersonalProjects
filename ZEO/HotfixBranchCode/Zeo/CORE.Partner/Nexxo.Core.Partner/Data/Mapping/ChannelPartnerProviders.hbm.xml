<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data">

	<class name ="ChannelPartnerProductProvider" table ="tChannelPartnerProductProcessorsMapping" lazy="false">
		<id name="rowguid" column="ChannelPartnerProductProcessorsMappingPK">
			<generator class="guid"/>
		</id>
		<property name="Id">
			<column name="ChannelPartnerProductProcessorsMappingID" not-null="true" sql-type="bigint IDENTITY(1,1)"/>
		</property>

		<many-to-one insert="false" update="false" lazy="false" name="ChannelPartner">
			<column name="ChannelPartnerId" sql-type="uniqueidentifier" not-null="false" />
		</many-to-one>

		<many-to-one insert="false" update="false" lazy="false" name="ProductProcessor">
			<column name="ProductProcessorId" sql-type="uniqueidentifier" not-null="false" />
		</many-to-one>

		<property name="Sequence" column="Sequence"></property>
		<property name="IsTnCForcePrintRequired" column="IsTnCForcePrintRequired"></property>
		<property name="CheckEntryType"/>
		<property name="MinimumTransactAge" column="MinimumTransactAge"/>
		<property name="CardExpiryPeriod" column="CardExpiryPeriod"></property>
		<property name="DTServerCreate" column="DTServerCreate" not-null="true" update="false" />
		<property name="DTServerLastModified" column="DTServerLastModified" not-null="false" insert="false" update="false" />
	</class>

	<class name ="ProductProcessor" table ="tProductProcessorsMapping" lazy="false">
		<id name="rowguid" column="ProductProcessorsMappingPK">
			<generator class="guid"/>
		</id>
		<property name="Id">
			<column name="ProductProcessorsMappingId" not-null="true" sql-type="bigint IDENTITY(1,1)"/>
		</property>

		<many-to-one insert="false" update="false" lazy="false" name="Product">
			<column name="ProductId" sql-type="uniqueidentifier" not-null="false" />
		</many-to-one>

		<many-to-one insert="false" update="false" lazy="false" name="Processor">
			<column name="ProcessorId" sql-type="uniqueidentifier" not-null="false" />
		</many-to-one>
		<property name="Code" />
		<property name="IsSSNRequired" />
		<property name="IsSWBRequired" />
		<property name="CanParkReceiveMoney" />
		<property name="ReceiptCopies" />
		<property name="ReceiptReprintCopies" />
		<property name="DTServerCreate" column="DTServerCreate" not-null="true" update="false" />
		<property name="DTServerLastModified" column="DTServerLastModified" not-null="false" insert="false" update="false" />


	</class>

	<class name="Processor" table="tProcessors" lazy="false">
		<id name="rowguid" column="ProcessorsPK">
			<generator class="guid"/>
		</id>
		<property name="Id">
			<column name="ProcessorsId" not-null="true" sql-type="bigint IDENTITY(1,1)"/>
		</property>
		<property name="Name" column="Name"  />
		<property name="DTServerCreate" column="DTServerCreate" not-null="true" update="false" />
		<property name="DTServerLastModified" column="DTServerLastModified" not-null="false" insert="false" update="false" />
	</class>

	<class name="Product" table="tProducts" lazy="false">
		<id name="rowguid" column="ProductsPK">
			<generator class="guid"/>
		</id>
		<property name="Id">
			<column name="ProductsId" not-null="true" sql-type="bigint IDENTITY(1,1)"/>
		</property>
		<property name="Name" column="Name"  />
		<property name="DTServerCreate" column="DTServerCreate" not-null="true" update="false" />
		<property name="DTServerLastModified" column="DTServerLastModified" not-null="false" insert="false" update="false" />
	</class>
</hibernate-mapping>