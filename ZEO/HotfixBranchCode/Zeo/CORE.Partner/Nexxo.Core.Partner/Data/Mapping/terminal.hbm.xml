<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data">
	<class name="Terminal" table="tTerminals">
		<id name="rowguid" column="TerminalPK">
			<generator class="guid"/>
		</id>
		<property name='Id' column='TerminalID' generated="insert" update="false" insert="false" />
		<property name='Name' column='`Name`' />
		<property name='IpAddress' column='`IpAddress`' />
		<property name='MacAddress' column='`MacAddress`' />
		<many-to-one name="Location" class="Location" column="LocationPK" not-null="false"/>
		<many-to-one name='PeripheralServer' class='NpsTerminal' column='`NpsTerminalPK`' />
		<many-to-one name='ChannelPartner' class='ChannelPartner' column='`ChannelPartnerPK`' />
		<property name="DTServerCreate" not-null="true" update="false"/>
		<property name="DTServerLastModified" not-null="false" insert="false"/>
		<property name="DTTerminalCreate" column="DTTerminalCreate" not-null="true" update="false"/>
		<property name="DTTerminalLastModified" column="DTTerminalLastModified" not-null="false"/>
	</class>
</hibernate-mapping>