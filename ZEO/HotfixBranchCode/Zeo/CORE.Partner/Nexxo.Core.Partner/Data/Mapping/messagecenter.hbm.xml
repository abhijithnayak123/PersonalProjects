<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MGI.Core.Partner" namespace="MGI.Core.Partner.Data">
	<class name='AgentMessage' table='tMessageCenter'>
		<id name="rowguid" column="MessageCenterPK">
			<generator class="guid"/>
		</id>
		<property name='Id' column='MessageCenterID' generated="insert" update="false" insert="false" />		
		<property name='IsParked' column='IsParked' />
		<property name='IsActive' column='IsActive' />
		<many-to-one name='Agent' class='MGI.Core.Partner.Data.UserDetails' column='AgentId' />
		<many-to-one name='Transaction' class='MGI.Core.Partner.Data.Transactions.Check' column='TxnId' />
		<property name="DTTerminalCreate" not-null="true" update="false"/>
		<property name="DTTerminalLastModified" not-null="false" insert="false"/>
		<property name="DTServerCreate" not-null="true" update="false"/>
		<property name="DTServerLastModified" not-null="false" insert="false"/>
	</class>
</hibernate-mapping>