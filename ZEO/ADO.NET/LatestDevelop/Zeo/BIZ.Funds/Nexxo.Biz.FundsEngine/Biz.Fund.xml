<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns:db="http://www.springframework.net/database"
		 xmlns:tx="http://www.springframework.net/tx"
		 xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

	<import resource="assembly://MGI.Cxn.Fund.Visa/MGI.Cxn.Fund.Visa/MGI.Cxn.Fund.Visa.Spring.xml"/>
	<import resource="assembly://MGI.Core.CXE/MGI.Core.CXE/CXESpring.xml"/>
	<import resource="assembly://MGI.Core.Partner/MGI.Core.Partner/Core.Partner.Services.Spring.xml"/>
	<import resource="assembly://MGI.Biz.Compliance/MGI.Biz.Compliance/Biz.Compliance.Services.Spring.xml"/>

	<object id="ThrowsBizFundsEngineExceptionAdvice" type="MGI.Biz.FundsEngine.Impl.Advice.ThrowsBizFundsEngineExceptionAdvice, MGI.Biz.FundsEngine"/>

	<object id="InterceptedBIZFundsEngine" type="MGI.Biz.FundsEngine.Impl.FundsEngineImpl, MGI.Biz.FundsEngine">
		<property name="CXEFundsService" ref="CXEFundService" />
		<property name="CXECustomerService" ref="CXECustomerService" />
		<property name="CXEAccountService" ref="AccountService" />
		<property name="PTNRFundsService" ref="PartnerFundsService"/>
		<property name="CustomerSessionService" ref="CustomerSessionService"/>
		<property name="PTNRCustomerService" ref="Core.Partner.CustomerService"/>
		<property name="TerminalService" ref="PartnerTerminalService"/>
		<property name="FeeService" ref="PartnerFeeService"/>
		<property name="NexxoIdTypeService" ref="CoreNexxoDataStructuresService"/>
		<property name="LimitService" ref="BIZLimitService"/>
		<property name="PTNRChannelPartnerService" ref="CorePartnerChannelPartnerService"/>
		<!-- the processor router to route request for channel partner -->
		<property name="ProcessorRouter" ref="GPRProcessorRouter"/>
		<!-- the listeners for the Synovus GPR Add event...-->
		<property name="EventPublisher" ref="GPREventPublisher"/>
		<property name="MongoDBLogger" ref="TLoggerRef"></property>
	</object>

	<!-- The GPR Processors -->
	<object id="GPRProcessorRouter" type="MGI.Cxn.Common.Processor.Util.ProcessorRouter, MGI.Cxn.Common">
		<property name="Processors">
			<object id="GPRProcessorsDictionary" type="System.Collections.Generic.Dictionary&lt;string, MGI.Cxn.Common.Processor.Contract.IProcessor&gt;, mscorlib">
				<constructor-arg>
					<dictionary key-type="string" value-type="MGI.Cxn.Common.Processor.Contract.IProcessor">
						<entry key="Synovus">
							<ref object="CxnFundsVisaProcessor"/>
							<!-- imported from Visa impl xml -->
						</entry>
						<entry key="TCF">
							<ref object="CxnFundsVisaProcessor"/>
							<!-- imported from Visa impl xml -->
						</entry>
					</dictionary>
				</constructor-arg>
			</object>
		</property>

		<!-- The Fund Providers -->
		<property name="Providers">
			<object id="FundProvidersDictionary" type="System.Collections.Generic.Dictionary&lt;string, string&gt;, mscorlib">
				<constructor-arg>
					<dictionary key-type="string" value-type="string">
						<entry key="Synovus">
							<value type="string">Visa</value>
						</entry>
						<entry key="TCF">
							<value type="string">Visa</value>
						</entry>
					</dictionary>
				</constructor-arg>
			</object>
		</property>
		<!-- EO Fund Providers -->
	
	</object>
	<!-- EO GPR Processors -->

	<!--belongs to generic event handlers -->
	<object id="GPREventPublisher" type="MGI.Biz.Events.Impl.DefaultNexxoBizEventPublisher, MGI.Biz.Events">
		<property name="NexxoBizEventListeners" ref="SynovusGPRAddEventDictionary"/>
	</object>
	<!-- end of belongs to generic event handlers -->

	<!-- belongs to the Synovus Impl configuration file. Move there - TODO ... -->
	<object id="SynovusGPRAddEventDictionary"
			  type="System.Collections.Generic.Dictionary&lt;string, System.Collections.Generic.Dictionary&lt;string, MGI.Biz.Events.Contract.INexxoBizEventListener&gt;&gt;, mscorlib">
		<constructor-arg>
			<dictionary key-type="string" value-type="System.Collections.Generic.Dictionary&lt;string, MGI.Biz.Events.Contract.INexxoBizEventListener&gt;">
				<entry key="Synovus">
					<ref object="SynovusGPRAddEventListenerDictionary"/>
				</entry>
			</dictionary>
		</constructor-arg>
	</object>

	<object id="SynovusGPRAddEventListenerDictionary" type="System.Collections.Generic.Dictionary&lt;string, MGI.Biz.Events.Contract.INexxoBizEventListener&gt;, mscorlib">
		<constructor-arg>
			<dictionary key-type="string" value-type="MGI.Biz.Events.Contract.INexxoBizEventListener">
				<entry key="GPRAdd">
					<ref object="SynovusGPRAddEventListener"/>
				</entry>
			</dictionary>
		</constructor-arg>
	</object>	

	<!-- END OF -::- belongs to the Synovus Impl configuration file. Move there - TODO ... -->

	<object id="BIZFundsEngine" type="Spring.Aop.Framework.ProxyFactoryObject, Spring.Aop">
		<property name="Target" ref="InterceptedBIZFundsEngine"/>
		<property name="InterceptorNames">
			<list>
				<value>ThrowsBizFundsEngineExceptionAdvice</value>
			</list>
		</property>
	</object>
</objects>