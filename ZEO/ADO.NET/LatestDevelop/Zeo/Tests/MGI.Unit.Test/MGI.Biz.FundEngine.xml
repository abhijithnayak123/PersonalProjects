<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:db="http://www.springframework.net/database"
   xmlns:tx="http://www.springframework.net/tx"
   xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

	<object type="Spring.Objects.Factory.Config.PropertyPlaceholderConfigurer, Spring.Core">
		<property name="ConfigSections" value="databaseSettings"/>
	</object>
	<object id="transactionManager"
		type="Spring.Data.Core.TxScopeTransactionManager, Spring.Data">
		<property name="TransactionSynchronization" value="Always"/>
	</object>
	<tx:attribute-driven/>

	<object name="MockFundEngine" type="MGI.Unit.Test.MGI.Biz.FundsEngine.Test.MockData.MockFundEngine, MGI.Unit.Test"></object>
	
	<object id="InterceptedBIZFundsEngine" type="MGI.Biz.FundsEngine.Impl.FundsEngineImpl, MGI.Biz.FundsEngine">
		<property name="CXEFundsService" ref="CXEFundService" />
		<property name="CXECustomerService" ref="CXECustomerService" />
		<property name="CXEAccountService" ref="AccountService" />
		<property name="PTNRFundsService" ref="PartnerFundsService"/>
		<property name="CustomerSessionService" ref="CustomerSessionService"/>
		<property name="PTNRCustomerService" ref="Core.Partner.CustomerService"/>
		<property name="TerminalService" ref="PartnerTerminalService"/>
		<property name="FeeService" ref="PartnerFeeService"/>
		<property name="NexxoIdTypeService" ref="CoreNexxoDataStructuresService"/>
		<property name="LimitService" ref="BIZLimitService"/>
		<property name="PTNRChannelPartnerService" ref="CorePartnerChannelPartnerService"/>
		<!-- the processor router to route request for channel partner -->
		<property name="ProcessorRouter" ref="GPRProcessorRouter"/>
		<!-- the listeners for the Synovus GPR Add event...-->
		<property name="EventPublisher" ref="NexxoBizEventPublisher"/>
		<property name="MongoDBLogger" ref="TLoggerRef"></property>
	</object>


	<object id="CXEFundService" factory-object="MockFundEngine" factory-method="CreateInstanceFundService"></object>

	<object id="GPRProcessorRouterMock" factory-object="MockFundEngine" factory-method="CreateInstanceOfFundProcessor"></object>
	
	<!-- The GPR Processors -->
	<object id="GPRProcessorRouter" type="MGI.Cxn.Common.Processor.Util.ProcessorRouter, MGI.Cxn.Common">
		<property name="Processors">
			<object id="GPRProcessorsDictionary" type="System.Collections.Generic.Dictionary&lt;string, MGI.Cxn.Common.Processor.Contract.IProcessor&gt;, mscorlib">
				<constructor-arg>
					<dictionary key-type="string" value-type="MGI.Cxn.Common.Processor.Contract.IProcessor">
						<entry key="Centris">
							<ref object="GPRProcessorRouterMock"/>
							<!-- imported from F.View impl xml-->
						</entry>
						<entry key="Synovus">
							<ref object="GPRProcessorRouterMock"/>
							<!-- imported from TSys impl xml -->
						</entry>
						<entry key="TCF">
							<ref object="GPRProcessorRouterMock"/>
							<!-- imported from Visa impl xml -->
						</entry>
					</dictionary>
				</constructor-arg>
			</object>
		</property>

		<!-- The Fund Providers -->
		<property name="Providers">
			<object id="FundProvidersDictionary" type="System.Collections.Generic.Dictionary&lt;string, string&gt;, mscorlib">
				<constructor-arg>
					<dictionary key-type="string" value-type="string">
						<entry key="Synovus">
							<value type="string">Visa</value>
						</entry>
						<entry key="TCF">
							<value type="string">Visa</value>
						</entry>
						<entry key="Centris">
							<value type="string">FirstView</value>
						</entry>
					</dictionary>
				</constructor-arg>
			</object>
		</property>
		<!-- EO Fund Providers -->

	</object>
	<!-- EO GPR Processors -->
	
</objects>