	</object>

	<object id="ComplianceProgramService" type="MGI.Core.Compliance.Impl.ComplianceProgramServiceImpl, MGI.Core.Compliance">
		<property name="ComplianceProgramRepo" ref="NHibComplianceProgramRepo"/>
