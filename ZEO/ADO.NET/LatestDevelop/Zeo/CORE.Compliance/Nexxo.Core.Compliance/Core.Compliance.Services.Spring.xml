<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns:db="http://www.springframework.net/database"
		 xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

	<import resource="assembly://MGI.Core.Compliance/MGI.Core.Compliance/Core.Compliance.Db.Spring.xml" />

	<object id="NHibComplianceProgramRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.Compliance.Data.ComplianceProgram>, MGI.Common.DataAccess">
