	</object>

	<object id="NHibLimitFailureRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.Compliance.Data.LimitFailure>, MGI.Common.DataAccess">
		<property name="SessionFactory" ref="SessionFactoryCoreCompliance"/>
	</object>
	<object id="CoreComplianceLimitFailureService" type="MGI.Core.Compliance.Impl.LimitFailureServiceImpl, MGI.Core.Compliance">
		<property name="FailureRepo" ref="NHibLimitFailureRepo"/>
