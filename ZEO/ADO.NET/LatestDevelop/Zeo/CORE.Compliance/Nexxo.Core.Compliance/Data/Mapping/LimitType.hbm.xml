<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Compliance"
                   namespace="MGI.Core.Compliance.Data">
  <class name="LimitType" table="tLimitTypes">
    <id name="rowguid" column="LimitTypePK">
      <generator class="guid"/>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="LimitTypeID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>

    <many-to-one name="ComplianceProgram" class="MGI.Core.Compliance.Data.ComplianceProgram" column="ComplianceProgramPK"/>
    <property name="Name"/>

    <bag name="Limits" cascade="all-delete-orphan" lazy="false">
      <key column="LimitTypePK"/>
      <one-to-many class="MGI.Core.Compliance.Data.Limit"/>
    </bag>

    <property name="DTServerCreate" update="false"/>
    <property name="DTServerLastModified" insert="false" not-null="false"/>
  </class>
</hibernate-mapping>
