<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		     xmlns:db="http://www.springframework.net/database"
		     xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

  <import resource="assembly://MGI.Cxn.Customer.CCIS/MGI.Cxn.Customer.CCIS.Impl/Cxn.Customer.CCIS.Db.Spring.xml" />

  <object id="CCISConnectRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Customer.CCIS.Data.CCISConnect&gt;, MGI.Common.DataAccess">
    <property name="SessionFactory" ref="Cxn.Customer.CCIS.SessionFactory"></property>
  </object>

  <object id="CCISAccountRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Cxn.Customer.CCIS.Data.CCISAccount&gt;, MGI.Common.DataAccess">
    <property name="SessionFactory" ref="Cxn.Customer.CCIS.SessionFactory"></property>
  </object>

  <object id ="CCISProcessorDAL" type="MGI.Cxn.Customer.CCIS.Impl.ProcessorDAL, MGI.Cxn.Customer.CCIS">
    <property name ="CCISConnectRepo" ref="CCISConnectRepo"/>
    <property name ="CCISAccountRepo" ref="CCISAccountRepo"/>
    <property name="NLogger" ref="NLoggerRef"></property>
  </object>
  
  <object id ="CCISGateway" type="MGI.Cxn.Customer.CCIS.Impl.CCISGateway, MGI.Cxn.Customer.CCIS">
    <property name ="CCISProcessorDAL" ref="CCISProcessorDAL"/>
  </object>

</objects>