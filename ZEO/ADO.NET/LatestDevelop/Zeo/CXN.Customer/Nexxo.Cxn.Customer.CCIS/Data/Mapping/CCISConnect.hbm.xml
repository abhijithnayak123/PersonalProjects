<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping
  assembly="MGI.Cxn.Customer.CCIS"
  namespace="MGI.Cxn.Customer.CCIS.Data"
  xmlns="urn:nhibernate-mapping-2.2">

  <class name="CCISConnect" table="tCCISConnectsDb" mutable="false">
    <id name="rowguid" column="CCISConnectsPK">
      <generator class="guid"/>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="CCISConnectsID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>

    <property name="CustomerNumber" />
    <property name="CustomerTaxNumber" />
    <property name="PrimaryPhoneNumber" />
    <property name="SecondaryPhone" />
    <property name="LastName" />
    <property name="FirstName" />
    <property name="MiddleName" />
    <property name="MiddleName2" />
    <property name="AddressStreet" />
    <property name="AddressCity" />
    <property name="AddressState" />
    <property name="ZipCode" />
	  <property name="DateOfBirth">
		  <column name="DOB" sql-type="datetime" not-null="false" />
	  </property>
    <property name="MothersMaidenName" />
    <property name="Gender" />
    <property name="DriversLicenseNumber" />
    <property name="ExternalKey" />
    <property name="MetBankNumber" />
    <property name="ProgramId" />
    <property name="DTServerCreate" />
  </class>
</hibernate-mapping> 
