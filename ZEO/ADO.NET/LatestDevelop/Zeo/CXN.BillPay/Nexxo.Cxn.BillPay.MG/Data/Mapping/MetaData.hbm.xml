<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="MGI.Cxn.BillPay.MG" namespace="MGI.Cxn.BillPay.MG.Data" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Country" table="tMGram_Countries" lazy="true" >
		<id name="rowguid" column="MGCountryPK" />
		<property name="Id">
			<column name="MGCountryID" sql-type="bigint" not-null="true" />
		</property>
		<property name="Code">
			<column name="Code" sql-type="varchar" not-null="true" />
		</property>
		<property name="Name">
			<column name="Name" sql-type="varchar" not-null="true" />
		</property>
		<property name="Legacycode">
			<column name="LegacyCode" sql-type="varchar" not-null="true" />
		</property>
		<property name="Sendactive">
			<column name="SendActive" sql-type="bit" not-null="false" />
		</property>
		<property name="Receiveactive">
			<column name="ReceiveActive" sql-type="bit" not-null="false" />
		</property>
		<property name="Directedsendcountry">
			<column name="DirectedSendCountry" sql-type="bit" not-null="false" />
		</property>
		<property name="Mgdirectedsendcountry">
			<column name="MGDirectedSendCountry" sql-type="bit" not-null="false" />
		</property>
		<property name="DTServerCreate">
			<column name="DTServerCreate" sql-type="datetime" not-null="true" />
		</property>
		<property name="DTServerLastModified">
			<column name="DTServerLastModified" sql-type="datetime" not-null="false" />
		</property>
	</class>

	<class name="State" table="tMGram_States" lazy="true" >
		<id name="rowguid" column="MGStatePK" />
		<property name="Id">
			<column name="MGStateID" sql-type="bigint" not-null="true" />
		</property>
		<property name="Code">
			<column name="Code" sql-type="varchar" not-null="true" />
		</property>
		<property name="Name">
			<column name="Name" sql-type="varchar" not-null="true" />
		</property>
		<property name="Countrycode">
			<column name="CountryCode" sql-type="varchar" not-null="true" />
		</property>
		<property name="DTServerCreate">
			<column name="DTServerCreate" sql-type="datetime" not-null="true" />
		</property>
		<property name="DTServerLastModified">
			<column name="DTServerLastModified" sql-type="datetime" not-null="false" />
		</property>
	</class>
  
</hibernate-mapping>
