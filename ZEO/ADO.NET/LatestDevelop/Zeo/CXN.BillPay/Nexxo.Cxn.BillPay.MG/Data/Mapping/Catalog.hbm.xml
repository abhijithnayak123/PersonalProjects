<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Cxn.BillPay.MG"
                   namespace="MGI.Cxn.BillPay.MG.Data">

  <class name="Catalog" table="tMGram_Catalog" lazy="true" >
    <id name="rowguid" column="MGCatalogPK">
      <generator class="guid" />
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="MGCatalogID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <property name="DTServerCreate">
      <column name="DTServerCreate" sql-type="datetime" not-null="true" />
    </property>
    <property name="DTServerLastModified">
      <column name="DTServerLastModified" sql-type="datetime" not-null="false" />
    </property>
    <property name="IsActive">
      <column name="IsActive" sql-type="bit" not-null="true" />
    </property>
    <property name="ChannelPartnerId">
      <column name="ChannelPartnerId" sql-type="bigint" not-null="true" />
    </property>
    <property name="ReceiveAgentId">
      <column name="ReceiveAgentId" sql-type="varchar" not-null="false" />
    </property>
    <property name="ReceiveCode">
      <column name="ReceiveCode" sql-type="varchar" not-null="false" />
    </property>
    <property name="BillerName">
      <column name="BillerName" sql-type="varchar" not-null="false" />
    </property>
    <property name="PoeSvcMsgENText">
      <column name="Poe_Svc_Msg_EN_Text" sql-type="varchar" not-null="false" />
    </property>
    <property name="PoeSvcMsgESText">
      <column name="Poe_Svc_Msg_ES_Text" sql-type="varchar" not-null="false" />
    </property>
    <property name="Keywords">
      <column name="Keywords" sql-type="varchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>