@model TCF.Channel.Zeo.Web.Models.EmploymentDetails

@{
    ViewBag.Title = TCF.Channel.Zeo.Web.App_GlobalResources.Nexxo.EmploymentDetails;
    Layout = "~/Views/Shared/_Menu.cshtml";
}
<script src="@Url.Content("~/Scripts/Customer/BaseCustomerValidator.js.axd")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/Customer/" + @Model.ChannelPartnerName + "CustomerValidator.js.axd")" type="text/javascript"></script>
<div align="center"><img src="@Url.Content("~/Content/images/Employment_" + @Model.ChannelPartnerName + ".png")"  usemap="#@Model.ChannelPartnerName"/></div>
@using (Html.BeginForm("EmploymentDetails", "CustomerRegistration", FormMethod.Post, new { id = "frmEmploymentDetails" }))
{
    @Html.ValidationSummary(true)
    @Html.Partial("_EmploymentDetails", Model)
    @Html.Partial("_EmploymentNavigation")
}
