	<xs:include schemaLocation="XRSIBasicTypes.xsd"/>
	<!-- This defines the Western Union Specific Classes that will used by the various transactions -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="channel">
		<xs:sequence>
			<xs:element minOccurs="0" name="type" type="xrsi:channel_type"/>
			<xs:element minOccurs="0" name="name">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="version">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="security">
		<xs:sequence>
			<xs:element minOccurs="0" name="client_ip" type="xrsi:ip_address"/>
			<xs:element minOccurs="0" name="session" type="xrsi:session"/>
			<!-- counters is moved to application -->
			<!-- <xs:element name="counters" type="xrsi:counter_details" minOccurs="0"/> -->
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="session_digest">
		<xs:sequence>
			<xs:element minOccurs="0" name="id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="data_digest">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="term_id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
			<xs:element minOccurs="0" name="timestamp">
			  <xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="0"/>
					<xs:maxLength value="18"/>
				</xs:restriction>
		     </xs:simpleType>	
		  </xs:element>		
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="session">
		<xs:sequence>
			<xs:element minOccurs="0" name="id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="200"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="new_id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="55"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="agent_account">
		<xs:sequence>
			<xs:element minOccurs="0" name="id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="general_name" type="xrsi:general_name"/>
			<xs:element minOccurs="0" name="address" type="xrsi:address"/>
			<xs:element minOccurs="0" name="code">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element maxOccurs="3" minOccurs="0" name="defined_data">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:annotation>
							<xs:documentation> EFP FID 259 (Agent Fields) </xs:documentation>
						</xs:annotation>
						<xs:minLength value="0"/>
						<xs:maxLength value="99"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="authorization_code">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:annotation>
							<xs:documentation> EFP FID 317 (Agent Fields) </xs:documentation>
						</xs:annotation>
						<xs:minLength value="0"/>
						<xs:maxLength value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="authorization_text">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:annotation>
							<xs:documentation> EFP FID 318 (Agent Fields) </xs:documentation>
						</xs:annotation>
						<xs:minLength value="0"/>
						<xs:maxLength value="73"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--   <xs:element name="currency_code" type="xrsi:currency" minOccurs="0"/>   -->
			<xs:element minOccurs="0" name="databases" type="xrsi:databases"/>
			<xs:element minOccurs="0" name="phone" type="xrsi:phone"/>
			<xs:element minOccurs="0" name="phone2" type="xrsi:phone"/>
			<xs:element minOccurs="0" name="status" type="xs:string"/>
			<xs:element minOccurs="0" name="contact_name" type="xrsi:general_name"/>
			<xs:element minOccurs="0" name="contact_phone" type="xrsi:phone"/>
			<xs:element minOccurs="0" name="memo" type="xrsi:agent_memo"/>
			<!--  <xs:element minOccurs="0" name="allowed_functions" type="xrsi:allowed_functions">-->
			<!--<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="12"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element> --> 
			<xs:element minOccurs="0" name="network_id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="agent_account_tables" type="xrsi:table_names"/>
			<xs:element minOccurs="0" name="terminal_id">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>MOM terminal code-MOM FID 2</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="table_names">
		<xs:sequence>
			<xs:element minOccurs="0" name="template">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
			<xs:element minOccurs="0" name="delivery_service">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
						
			<xs:element minOccurs="0" name="country_info">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
			<xs:element minOccurs="0" name="country">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="ckw">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="qqc">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="sup">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	
	<xs:complexType name="file_names">
		<xs:sequence>
			<xs:element minOccurs="0" name="ddl">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			
			<xs:element minOccurs="0" name="error_code">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
						
			<xs:element minOccurs="0" name="mexican_db">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="9"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
						
		</xs:sequence>
	</xs:complexType>
	
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="currency_databases">
		<xs:sequence>
			<xs:element maxOccurs="6" minOccurs="0" name="currency_database" type="xrsi:currency_database"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="currency_database">
		<xs:complexContent>
			<xs:extension base="xrsi:collection_base">
				<xs:sequence>
					<xs:element minOccurs="0" name="currency_code" type="xrsi:country_currency_info"/>
					<xs:element maxOccurs="12" minOccurs="0" name="database" type="xrsi:database"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="terminal">
		<xs:sequence>
			<xs:element minOccurs="0" name="name">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>Pathway terminal name-MOM FID 1 or 3</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="16"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!-- <xs:element name="code" minOccurs="0">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>MOM terminal code-MOM FID 2</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element> -->
			<xs:element minOccurs="0" name="id">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>MOM terminal code-MOM FID 2</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="alias">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="device_id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="2"/>
						<xs:enumeration value="PC"/>
						<xs:enumeration value="XP"/>
						<xs:enumeration value="IP"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="model_id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="2"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="ip_address" type="xrsi:ip_address"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="operator">
		<xs:sequence>
			<xs:element minOccurs="0" name="id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="name" type="xrsi:general_name"/>
			<xs:element minOccurs="0" name="password" type="xrsi:password_long"/>
			<xs:element minOccurs="0" name="new_password" type="xrsi:password_long"/>
			<xs:element minOccurs="0" name="alias">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="20"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="network_id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="6"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--  <xs:element minOccurs="0" name="allowed_functions" type="xrsi:allowed_functions"/>-->
			<xs:element minOccurs="0" name="security_flags" type="xrsi:security_flags"/>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="updating_operator">
		<xs:sequence>
			<xs:element minOccurs="0" name="update_flag" type="xrsi:flag_update"/>
			<xs:element minOccurs="0" name="id">
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="8"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs="0" name="name" type="xrsi:string"/>
			<xs:element minOccurs="0" name="password" type="xrsi:password"/>
			<xs:element minOccurs="0" name="terminal_id">
				<xs:simpleType>
					<xs:annotation>
						<xs:documentation>MOM terminal code-MOM FID 2</xs:documentation>
					</xs:annotation>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="4"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="collection_base">
		<xs:attribute name="more" type="xrsi:yes_no"/>
		<xs:attribute name="context" type="xrsi:context"/>
		<!--<xs:attribute name="find_number" type="xrsi:count"/>-->
	</xs:complexType>
	<!--   Added name_type as attribute for general_name  -->
	<xs:complexType name="name_type_base">
		<xs:attribute name="name_type" type="xrsi:name_type"/>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="transaction_base">
		<xs:complexContent>	
		<xs:extension base="xrsi:gbs_request_transaction_base">
			<xs:sequence>
				<xs:element minOccurs="0" name="channel" type="xrsi:channel"/>
				<xs:element minOccurs="0" name="security" type="xrsi:security"/>
				<xs:element minOccurs="0" name="instant_notification" type="xrsi:instant_notification"/>
				<xs:element minOccurs="0" name="domain" type="xrsi:domain"/>
				<xs:element minOccurs="0" name="wallet_reference_no" type="xrsi:wallet_reference_no"/>
				<xs:element minOccurs="0" name="session_digest" type="xrsi:session_digest"/>
				<!--	<xs:element name="agent_account" type="xrsi:agent_account"/>
				<xs:element name="pc_full_version_number" type="xrsi:pc_full_version_number"/>-->
			</xs:sequence>
			</xs:extension>
			</xs:complexContent>	
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="request_transaction_base">
		<xs:complexContent>
			<xs:extension base="xrsi:transaction_base">
				<xs:sequence>
					<xs:element minOccurs="0" name="agent_account" type="xrsi:agent_account"/>
					<xs:element minOccurs="0" name="operator" type="xrsi:operator"/>
					<xs:element minOccurs="0" name="terminal" type="xrsi:terminal"/>
					<xs:element minOccurs="0" name="foreign-system-reference-no" type="xrsi:reference_no"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="store_request_transaction_base">
		<xs:complexContent>
			<xs:extension base="xrsi:request_transaction_base">
				<xs:sequence>
					<xs:element minOccurs="0" name="remote_agent_account" type="xrsi:remote_account"/>
					<xs:element minOccurs="0" name="remote_operator" type="xrsi:operator"/>
					<xs:element minOccurs="0" name="remote_terminal" type="xrsi:terminal"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="reply_transaction_base">
		<xs:complexContent>
			<xs:extension base="xrsi:transaction_base">
				<xs:sequence>
					<xs:element minOccurs="0" name="advisory_text">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="0"/>
								<xs:maxLength value="80"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="general_name">
		<xs:complexContent>
			<xs:extension base="xrsi:name_type_base">
				<xs:sequence>
					<xs:element minOccurs="0" name="name_raw" type="xrsi:name_raw"/>
					<xs:element minOccurs="0" name="name_prefix" type="xrsi:name_prefix"/>
					<xs:group minOccurs="0" ref="xrsi:name_group"/>
					<xs:element minOccurs="0" name="name_suffix" type="xrsi:name_suffix"/>
					<xs:element minOccurs="0" name="secondary_given_name" type="xrsi:first_name"/>
					<xs:element minOccurs="0" name="secondary_paternal_name" type="xrsi:paternal_name"/>
					<xs:element minOccurs="0" name="secondary_maternal_name" type="xrsi:maternal_name"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:group name="name_group">
		<xs:choice>
			<xs:group minOccurs="0" ref="xrsi:individual_name"/>
			<xs:group minOccurs="0" ref="xrsi:organization_name"/>
			<xs:group minOccurs="0" ref="xrsi:mexican_name"/>
		</xs:choice>
	</xs:group>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:group name="individual_name">
		<xs:sequence>
			<xs:element minOccurs="0" name="first_name" type="xrsi:first_name"/>
			<xs:element minOccurs="0" name="middle_name" type="xrsi:middle_name"/>
			<xs:element minOccurs="0" name="last_name" type="xrsi:last_name"/>
		</xs:sequence>
	</xs:group>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:group name="mexican_name">
		<xs:sequence>
			<xs:element minOccurs="0" name="given_name" type="xrsi:first_name"/>
			<xs:element minOccurs="0" name="paternal_name" type="xrsi:paternal_name"/>
			<xs:element minOccurs="0" name="maternal_name" type="xrsi:maternal_name"/>
		</xs:sequence>
	</xs:group>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:group name="organization_name">
		<xs:sequence>
			<xs:element minOccurs="0" name="business_name" type="xrsi:business_name"/>
			<xs:element minOccurs="0" name="attention" type="xrsi:attention"/>
		</xs:sequence>
	</xs:group>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~  -->
	<xs:complexType name="address">
		<xs:sequence>
			<xs:element minOccurs="0" name="addr_line1" type="xrsi:address_line_long"/>
			<xs:element minOccurs="0" name="addr_line2" type="xrsi:address_line_long"/>
			<xs:element minOccurs="0" name="city" type="xrsi:city"/>
			<xs:element minOccurs="0" name="state" type="xrsi:state_province"/>
			<xs:element minOccurs="0" name="postal_code" type="xrsi:postal_code"/>
			<xs:choice>
