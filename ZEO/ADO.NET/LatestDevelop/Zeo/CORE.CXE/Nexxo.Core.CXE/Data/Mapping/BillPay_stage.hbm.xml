<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.CXE"
                   namespace="MGI.Core.CXE.Data.Transactions.Stage" auto-import="false">

  <class name="BillPay" table="tTxn_BillPay_Stage" dynamic-update="true" >
    <id name="rowguid" column="BillPayPK" >
      <generator class="guid" />
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="BillPayID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <property name="Amount" />
    <property name="Fee" />
    <many-to-one name="Account" class="MGI.Core.CXE.Data.Account" column="AccountPK" insert="true" update="false" cascade ="none" not-null ="true" />
    <property name="Status" />
    <property name="ProductId"/>
    <property name="AccountNumber"/>
    <property name="ConfirmationNumber"/>
    <property name="DTServerCreate" update="false"/>
    <property name="DTServerLastModified" insert="false" not-null="false"/>
    <property name="DTTerminalCreate" update="false"/>
    <property name="DTTerminalLastModified" insert="false" not-null="false"/>
  </class>

</hibernate-mapping>