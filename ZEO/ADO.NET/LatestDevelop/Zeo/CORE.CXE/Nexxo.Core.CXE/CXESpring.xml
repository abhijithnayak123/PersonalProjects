<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xmlns:db="http://www.springframework.net/database"
   xmlns:tx="http://www.springframework.net/tx"
   xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

  <db:provider id="CXEDbProvider"
        provider="SqlServer-2.0"
        connectionString="${PTNRDBConnection.connectionString}"/>
  
  <import resource="assembly://MGI.Common/MGI.Common/MGICommon.xml"/>

  <object id="SessionFactoryCXE" type="Spring.Data.NHibernate.LocalSessionFactoryObject, Spring.Data.NHibernate32">
    <property name="DbProvider" ref="CXEDbProvider"/>
    <property name="MappingAssemblies">
      <list>
        <value>MGI.Core.CXE</value>
      </list>
    </property>
    <property name="HibernateProperties">
      <dictionary>
        <entry key="connection.provider" value="NHibernate.Connection.DriverConnectionProvider" />
        <entry key="dialect" value="NHibernate.Dialect.MsSql2008Dialect" />
        <entry key="connection.driver_class" value="NHibernate.Driver.SqlClientDriver" />
        <entry key="show_sql" value="true" />
      </dictionary>
    </property>
    <property name="EventListeners">
      <dictionary>
        <!--<entry key="PostInsert" value-ref="nexxoauditEventListenerCXE"/>
        <entry key="PostUpdate" value-ref="nexxoauditEventListenerCXE"/>
        <entry key="PostDelete" value-ref="nexxoauditEventListenerCXE"/>-->
      </dictionary>
    </property>
    <property name="ExposeTransactionAwareSessionFactory" value="true"/>
  </object>
  <object id="CXECustomerRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Customer&gt;,  MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object >
  
  <object id="CashCommitRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Commit.Cash&gt;,  MGI.Common.DataAccess"  >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  <object id="CashStageRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Stage.Cash&gt;,  MGI.Common.DataAccess"  >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  
  <object id="CheckStageRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Stage.Check&gt;,  MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  <object id="CheckCommitRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Commit.Check&gt;,  MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  <object id="FundsCommitRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Commit.Funds&gt;, MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  <object id="FundsStageRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Stage.Funds&gt;, MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  <object id="MoneyTransferCommitRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Commit.MoneyTransfer&gt;, MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  <object id="MoneyTransferStageRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Stage.MoneyTransfer&gt;, MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>

  <object id="AccountRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Account&gt;, MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>

  <object id="BillPayStageRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Stage.BillPay&gt;,  MGI.Common.DataAccess"  >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  <object id="BillPayCommitRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Commit.BillPay&gt;,  MGI.Common.DataAccess"  >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>

  <object id="CustomerPreferedProductRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.CustomerPreferedProduct&gt;,  MGI.Common.DataAccess" autowire="byType" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  
  <object id="MoneyOrderStageRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Stage.MoneyOrder&gt;,  MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  <object id="MoneyOrderCommitRepo" type="MGI.Common.DataAccess.Impl.NHibRepository&lt;MGI.Core.CXE.Data.Transactions.Commit.MoneyOrder&gt;,  MGI.Common.DataAccess" >
    <property name="SessionFactory" ref="SessionFactoryCXE"></property>
  </object>
  <object id="CXEFundService" type="MGI.Core.CXE.Impl.FundServiceImpl, MGI.Core.CXE">
    <property name="FundsStageRepo" ref="FundsStageRepo"></property>
    <property name="FundsCommitRepo" ref="FundsCommitRepo"></property>
    <property name="NLogger" ref="NLoggerRef"></property>
	<property name="MongoDBLogger" ref="TLoggerRef"/>
  </object>

  <object id="CXEMoneyTransferService" type="MGI.Core.CXE.Impl.MoneyTransferServiceImpl, MGI.Core.CXE">
    <property name="MoneyTransferStageRepo" ref="MoneyTransferStageRepo"></property>
    <property name="MoneyTransferCommitRepo" ref="MoneyTransferCommitRepo"></property>
    <property name="NLogger" ref="NLoggerRef"></property>
    <property name="MongoDBLogger" ref="TLoggerRef"/>
  </object>

  <object id="AccountService" type="MGI.Core.CXE.Impl.AccountServiceImpl, MGI.Core.CXE">
    <property name="AccountRepo" ref="AccountRepo"></property>
  </object>

  <object id="CXEBillPayService" type="MGI.Core.CXE.Impl.BillPayServiceImpl, MGI.Core.CXE">
    <property name="BillPayStageRepo" ref="BillPayStageRepo" />
    <property name="BillPayCommitRepo" ref="BillPayCommitRepo" />
    <property name="BPDataProtectionSvc" ref="CXNBPDataProtection${DataProtectionService}"/>
    <property name="MongoDBLogger" ref="TLoggerRef"></property>
    <property name="NLogger" ref="NLoggerRef"></property>
  </object>

  <object id="CXEBillPaySetup" type="MGI.Core.CXE.Impl.BillPaySetupServiceImpl, MGI.Core.CXE">
    <property name="CustomerPreferedProductRepo" ref="CustomerPreferedProductRepo" />
    <property name="MongoDBLogger" ref="TLoggerRef"></property>
    <property name="NLogger" ref="NLoggerRef"></property>
  </object>

  <object id="CXECheckService" type="MGI.Core.CXE.Impl.CheckServiceImpl, MGI.Core.CXE">
    <property name="CheckStageRepo" ref="CheckStageRepo" />
    <property name="CheckCommitRepo" ref="CheckCommitRepo" />
    <property name="NLogger" ref="NLoggerRef"></property>
	<property name="MongoDBLogger" ref="TLoggerRef"></property>
  </object>
  
  <object id="CXECashService" type="MGI.Core.CXE.Impl.CashServiceImpl, MGI.Core.CXE">
    <property name="CashCommitRepo" ref="CashCommitRepo" />
    <property name="CashStageRepo" ref="CashStageRepo" />
    <property name="NLogger" ref="NLoggerRef"></property>
	<property name="MongoDBLogger" ref="TLoggerRef"></property>
  </object>

  <object id="CXECustomerService" type="MGI.Core.CXE.Impl.CustomerServiceImpl, MGI.Core.CXE">
    <property name="CustomerRepo" ref="CXECustomerRepo" />
    <property name="NLogger" ref="NLoggerRef"></property>
    <property name="MongoDBLogger" ref="TLoggerRef"/>
  </object>
  <object id="CXEMoneyOrderService" type="MGI.Core.CXE.Impl.MoneyOrderServiceImpl, MGI.Core.CXE">
    <property name="MoneyOrderStageRepo" ref="MoneyOrderStageRepo" />
    <property name="MoneyOrderCommitRepo" ref="MoneyOrderCommitRepo" />
    <property name="NLogger" ref="NLoggerRef"></property>
	<property name="MongoDBLogger" ref="TLoggerRef"></property>
  </object>
  <object id="CXNBPDataProtectionSimulator" type="MGI.Common.DataProtection.Impl.DataProtectionSimulator, MGI.Common.DataProtection"/>
  <object id="CXNBPDataProtectionService" type="MGI.Common.DataProtection.Impl.DataProtectionService, MGI.Common.DataProtection"/>
</objects>

