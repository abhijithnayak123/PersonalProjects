    <property name="State" column="State" />
    <property name="ZipCode" column="ZipCode" />
    <property name="ChannelPartnerId" column="ChannelPartnerId" />
    <property name="PhoneNumber" column="PhoneNumber" />
    <property name="TimezoneID" column="TimezoneID" />
    <property name="DTCreate" column="DTServerCreate" not-null="true" update="false" />
    <property name="DTLastMod" column="DTServerLastModified" not-null="true" insert="false"/>
  </class>
</hibernate-mapping>