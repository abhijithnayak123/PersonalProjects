<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data">
  <class name="Country" table="tCountries" lazy="true" >
    <id name="Id">
      <column name="CountryPK" sql-type="uniqueidentifier" not-null="true" />
    </id>
    <property name="Code">
      <column name="Code" sql-type="int" not-null="true" />
    </property>
    <property name="Name">
      <column name="Name" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Abbr2">
      <column name="Abbr2" sql-type="char" not-null="true" />
    </property>
    <property name="Abbr3">
      <column name="Abbr3" sql-type="char" not-null="true" />
    </property>
    <property name="XRate">
      <column name="xRate" sql-type="money" not-null="false" />
    </property>
    <property name="Dtrateasof">
      <column name="DTRateAsOf" sql-type="datetime" not-null="false" />
    </property>
    <property name="CurrencyName">
      <column name="Currency_Name" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CurrCdAlpha">
      <column name="Curr_CD_Alpha" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CurrCdNum">
      <column name="Curr_CD_Num" sql-type="int" not-null="false" />
    </property>
    <property name="Symbol">
      <column name="Symbol" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="DTServerLastModified">
      <column name="DTServerLastModified" sql-type="datetime" not-null="false" />
    </property>
    <property name="DTServerCreate">
      <column name="DTServerCreate" sql-type="datetime" not-null="false" />
    </property>
    <property name="CallingCode">
      <column name="CallingCode" sql-type="int" not-null="false" />
    </property>
    <property name="IsCOB">
      <column name="IsCOB" sql-type="bit" not-null="false" />
    </property>
    <property name="DebitAllowed">
      <column name="DebitAllowed" sql-type="bit" not-null="false" />
    </property>
  </class>
</hibernate-mapping>