<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="MGI.Core.Partner" namespace="MGI.Core.Partner.Data">
	<class name="Occupation" table="tOccupations" lazy="true" >

		<id name="rowguid" column="OccupationsPK">
			<generator class="guid"/>
		</id>
		<property name="Id" column="OccupationsID" generated="insert" update="false" insert="false" />
		<property name="Code">
			<column name="Code" sql-type="varchar" not-null="true" />
		</property>
		<property name="Name">
			<column name="Name" sql-type="varchar" not-null="true" />
		</property>
		<property name="IsActive">
			<column name="IsActive" sql-type="bit" not-null="true" />
		</property>
		<property name="DTServerCreate">
			<column name="DTServerCreate" sql-type="datetime" not-null="true" />
		</property>
		<property name="DTServerLastModified">
			<column name="DTServerLastModified" sql-type="datetime" not-null="false" />
		</property>
	</class>
</hibernate-mapping>