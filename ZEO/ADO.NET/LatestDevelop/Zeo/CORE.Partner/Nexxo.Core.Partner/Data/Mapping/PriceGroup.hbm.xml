<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data">
	
	<class name="PricingGroup" table="tPricingGroups" >
		<id name="rowguid">
			<column name="PricingGroupPK" sql-type="uniqueidentifier" not-null="true" />
		</id>

		<property name="Id" column="PricingGroupsID" generated="insert" update="false" insert="false"/>

		<property name="Name">
			<column name="PricingGroupName" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="DTServerCreate">
			<column name="DTServerCreate" sql-type="datetime" not-null="true" />
		</property>
		
		<property name="DTServerLastModified">
			<column name="DTServerLastModified" sql-type="datetime" not-null="false" />
		</property>
		<property name="DTTerminalCreate">
			<column name="DTTerminalCreate" sql-type="datetime" not-null="true" />
		</property>
		<property name="DTTerminalLastModified">
			<column name="DTTerminalLastModified" sql-type="datetime" not-null="false" />
		</property>
		<bag name="ChannelPartnerPricings" inverse="true" cascade="none">
			<key column="PricingGroupPK" />
			<one-to-many class="ChannelPartnerPricing" />
		</bag>
		<bag name="Pricings" inverse="true" cascade="none">
			<key column="PricingGroupPK" />
			<one-to-many class="Pricing" />
		</bag>
	</class>
</hibernate-mapping>