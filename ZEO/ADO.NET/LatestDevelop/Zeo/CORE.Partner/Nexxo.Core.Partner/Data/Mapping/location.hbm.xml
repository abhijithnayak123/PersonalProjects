<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data">


  <class name="Location" table="tLocations" >
    <id name="rowguid" column="LocationPK" >
    </id>
    <property name="Id" column="LocationID" generated="insert" update="false" insert="false"/>
    <property name="BankID" column="BankID"  />
    <property name="BranchID" column="BranchID"  />
	<property name="LocationIdentifier" column="LocationIdentifier"  />
    <property name="LocationName" column="LocationName"  />
    <property name="IsActive" column="IsActive" />
    <property name="Address1" column="Address1" />
    <property name="Address2" column="Address2" />
    <property name="City" column="City" />
    <property name="State" column="State" />
    <property name="ZipCode" column="ZipCode" />
    <property name="ChannelPartnerId" column="ChannelPartnerId" />
    <property name="PhoneNumber" column="PhoneNumber" />
    <property name="TimezoneID" column="TimezoneID" />
    <property name="DTServerCreate" column="DTServerCreate" not-null="true" update="false" />
    <property name="DTServerLastModified" column="DTServerLastModified" not-null="true" insert="false"/>
	  <property name="NoOfCounterIDs" column="NoOfCounterIDs"  />
		<property name="DTTerminalCreate" column="DTTerminalCreate" not-null="true" update="false"/>
		<property name="DTTerminalLastModified" column="DTTerminalLastModified" not-null="false" />
    <bag name="LocationProcessorCredentials" cascade="all-delete-orphan" lazy="false">
      <key column="LocationPK"/>
      <one-to-many class="LocationProcessorCredentials"/>
    </bag>
  </class>

</hibernate-mapping>
