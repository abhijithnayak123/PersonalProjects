<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping assembly="MGI.Cxn.Fund.Visa" namespace="MGI.Cxn.Fund.Visa.Data" xmlns="urn:nhibernate-mapping-2.2">
	<class name="CardShippingTypes" table="tVisa_CardShippingTypes" lazy="true" >
		<id name="rowguid" column="ShippingTypePK">
			<generator class="guid" />
		</id>
		<property name="Id">
			<column name="CardShippingTypeId" sql-type="bigint" not-null="true" />
		</property>
		<property name="Code">
			<column name="Code" sql-type="nvarchar" not-null="true" />
		</property>
		<property name="Name">
			<column name="Name" sql-type="nvarchar" not-null="true" />
		</property>
		<property name="DTServerCreate">
			<column name="DTServerCreate" sql-type="datetime" not-null="true" />
		</property>
		<property name="DTServerLastModified">
			<column name="DTServerLastModified" sql-type="datetime" not-null="false" />
		</property>
	    <property name="Active">
	      <column name="Active" sql-type="bit" not-null="false" />
	    </property>
		<!--<bag name="ChannelPartnerShippingTypeMapping" inverse="true" cascade="none">
			<key column="ShippingTypePK" />
			<one-to-many class="ChannelPartnerShippingTypeMapping" />
		</bag>-->
	</class>
</hibernate-mapping>