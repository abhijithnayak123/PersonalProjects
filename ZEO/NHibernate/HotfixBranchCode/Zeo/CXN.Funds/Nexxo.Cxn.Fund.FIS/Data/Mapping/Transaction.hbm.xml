<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MGI.Cxn.Fund.FIS" namespace="MGI.Cxn.Fund.FIS.Data" xmlns="urn:nhibernate-mapping-2.2">
  <class name="FISTrx" table="tFIS_GPR_Trx">
    <id name="rowguid">
      <generator class="guid"/>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="Id" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>

    <many-to-one name="Account" class="MGI.Cxn.Fund.FIS.Data.FISAccount" column="AccountPK" insert="true" update="false" cascade="none" not-null="true" />
    <property name="TransactionType"/>
    <property name="Amount"/>
    <property name="Fee"/>
    <property name="Description"/>
    <property name="DTLocalTransaction"/>
    <property name="DTTransmission"/>
    <property name="Status"/>
    <property name="ErrorCode"/>
    <property name="ErrorMsg"/>
    <property name="ConfirmationId"/>
    <property name="Balance"/>
    <property name="DTCreate"/>
    <property name="DTLastMod"/>
    <property name="ChannelPartnerID"/>
    <property name="DTServerCreate"/>
    <property name="DTServerLastMod"/>
  </class>
</hibernate-mapping>