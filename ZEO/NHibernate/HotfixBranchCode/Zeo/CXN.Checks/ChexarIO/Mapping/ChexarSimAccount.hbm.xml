<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="ChexarIO"
    namespace="ChexarIO.SimEntities">

  <class name="ChexarSimAccount" table="tChxrSim_Account" >
    <id name="rowguid" column="ChxrSimAccountPK">
      <generator class="guid">
      </generator>
    </id>
    <property name="Badge" generated="insert" update="false" insert="false">
      <column name="Badge" not-null="true" sql-type="bigint IDENTITY(10000000,1)"/>
    </property>
    <property name="FName" />
    <property name="LName" />
    <property name="ITIN" />
    <property name="SSN" />
	  <property name="DateOfBirth">
		  <column name="DOB" sql-type="datetime" not-null="false" />
	  </property>
    <property name="Address1" />
    <property name="Address2" />
    <property name="City" />
    <property name="State" />
    <property name="Zip" />
    <property name="Phone" />
    <property name="Occupation" />
    <property name="Employer" />
    <property name="EmployerPhone" />
	<property name="GovernmentId">
	  <column name="ChxrSimAccountID" sql-type="nvarchar" not-null="false" />	  </property>
    <property name="IDType" />
    <property name="IDCountry" />
    <property name="IDExpDate" />
    <property name="IDImage" />
    <property name="CardNumber" />
    <property name="CustomerScore" />
    <property name="DTServerCreate" update="false"/>
    <property name="DTServerLastModified" insert="false" not-null="false"/>
	<property name="IDCode"/>
  </class>
</hibernate-mapping>