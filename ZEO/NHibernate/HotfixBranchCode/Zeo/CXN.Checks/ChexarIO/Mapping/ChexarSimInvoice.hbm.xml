<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="ChexarIO"
    namespace="ChexarIO.SimEntities">

  <class name="ChexarSimInvoice" table="tChxrSim_Invoice" dynamic-update="true" >
    <id name="rowguid" column="ChxrSimInvoicePK" >
      <generator class="guid">
      </generator>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="ChxrSimInvoiceID" not-null="true" sql-type="int IDENTITY(100000,1)"/>
    </property>
    <property name="TicketId" />
    <property name="Amount" />
    <property name="Fee" />
    <property name="CheckType" />
    <property name="Status" />
    <property name="WaitTime" />
    <property name="DeclineId" />
    <property name="DeclineReason" />

    <many-to-one name="Account" class="ChexarIO.SimEntities.ChexarSimAccount"
             column="ChxrSimAccountPK" insert="true" update="false" cascade="none" not-null="true" />

    <property name="DTServerCreate" update="false"/>
    <property name="DTServerLastModified" insert="false" not-null="false"/>
  </class>
</hibernate-mapping>