<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MGI.Cxn.Check.Certegy" namespace="MGI.Cxn.Check.Certegy.Data" xmlns="urn:nhibernate-mapping-2.2">
	<class name="Credential" table="tCertegy_Credential" lazy="true" >
		<id name="rowguid" column="CertegyCredentialPK">
			<generator class="guid" />
		</id>
		<property name="Id">
			<column name="CertegyCredentialId" sql-type="bigint" not-null="true" />
		</property>
		<property name="ServiceUrl">
			<column name="ServiceUrl" sql-type="varchar" not-null="true" />
		</property>
		<property name="CertificateName">
			<column name="CertificateName" sql-type="varchar" not-null="true" />
		</property>
		<property name="Version">
			<column name="Version" sql-type="varchar" not-null="true" />
		</property>
		<property name="ChannelPartnerId">
			<column name="ChannelPartnerId" sql-type="bigint" not-null="true" unique="true" />
		</property>
		<property name="DeviceType">
			<column name="DeviceType" sql-type="char" not-null="true" />
		</property>
		<property name="DeviceIP">
			<column name="DeviceIP" sql-type="nvarchar" not-null="true" />
		</property>
		<property name="DTServerCreate">
			<column name="DTServerCreate" sql-type="datetime" not-null="true" />
		</property>
		<property name="DTServerLastModified">
			<column name="DTServerLastModified" sql-type="datetime" not-null="false" />
		</property>
	</class>
</hibernate-mapping>