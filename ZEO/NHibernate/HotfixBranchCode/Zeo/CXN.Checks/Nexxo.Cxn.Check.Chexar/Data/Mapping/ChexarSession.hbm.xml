<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
    assembly="MGI.Cxn.Check.Chexar"
    namespace="MGI.Cxn.Check.Chexar.Data">
  
    <class name="ChexarSession" table="tChxr_Session">
      <id name="rowguid" column="ChxrSessionPK">
        <generator class="guid"/>
      </id>
      <property name="Location"/>
      <property name="CompanyToken"/>
      <property name="EmployeeId"/>
      <property name="BranchId"/>

      <many-to-one name="Partner" class="MGI.Cxn.Check.Chexar.Data.ChexarPartner"
                   column="ChxrPartnerPK" insert="true" update="false" cascade="none" not-null="true" />


      <property name="DTTerminalCreate" update="false"/>
      <property name="DTTerminalLastModified" insert="false" not-null="false"/>
    </class>
</hibernate-mapping>