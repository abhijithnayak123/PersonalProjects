<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		 xmlns:db="http://www.springframework.net/database"
		 xmlns:tx="http://www.springframework.net/tx"
		 xsi:schemaLocation="http://www.springframework.net http://www.springframework.net/xsd/spring-objects.xsd">

  <import resource="assembly://MGI.Core.Compliance/MGI.Core.Compliance/Core.Compliance.Services.Spring.xml"/>
  <import resource="assembly://MGI.Core.CXE/MGI.Core.CXE/CXESpring.xml"/>
  <import resource="assembly://MGI.Core.Partner/MGI.Core.Partner/Core.Partner.Services.Spring.xml"/>

  <object id="ThrowsBizComplianceExceptionAdvice" type="MGI.Biz.Compliance.Impl.Advice.ThrowsBizComplianceExceptionAdvice, MGI.Biz.Compliance"/>

  <object name="InterceptedBizLimitService" type="MGI.Biz.Compliance.Impl.LimitServiceImpl, MGI.Biz.Compliance">
    <property name="CoreComplianceProgramService" ref="ComplianceProgramService"/>
    <property name="ComplianceTransactionService" ref="ComplianceTransactionService"/>
    <property name="PartnerCustomerSessionService" ref="CustomerSessionService"/>
		<property name="NLogger" ref="NLoggerRef"></property>
	  <property name="MongoDBLogger" ref="TLoggerRef"/>
  </object>

  <object id="BIZLimitService" type="Spring.Aop.Framework.ProxyFactoryObject, Spring.Aop">
    <property name="Target" ref="InterceptedBizLimitService"/>
    <property name="InterceptorNames">
      <list>
        <value>ThrowsBizComplianceExceptionAdvice</value>
      </list>
    </property>
  </object>

</objects>
