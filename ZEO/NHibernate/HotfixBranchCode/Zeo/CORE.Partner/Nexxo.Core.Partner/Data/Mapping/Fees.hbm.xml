<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data.Fees">

  <class name="CheckFee" table="tChannelPartnerFees_Check">
    <id name="rowguid" column="FeeCheckPK">
      <generator class="guid"/>
    </id>
    <many-to-one name="channelPartner" class="MGI.Core.Partner.Data.ChannelPartner" column="ChannelPartnerPK" />
    <property name="CheckType" column="CheckTypePK"/>
    <property name="FeeRate"/>
    <property name="FeeMinimum"/>
    <property name="DTServerCreate" not-null="true" update="false"/>
    <property name="DTServerLastModified" not-null="false" insert="false"/>
  </class>
  
  <class name="FundsFee" table="tChannelPartnerFees_Funds">
    <id name="rowguid" column="FeeFundsPK">
      <generator class="guid"/>
    </id>
    <many-to-one name="channelPartner" class="MGI.Core.Partner.Data.ChannelPartner" column="ChannelPartnerPK" />
    <property name="FundsType"/>
    <property name="Fee"/>
    <property name="DTServerCreate" not-null="true" update="false"/>
    <property name="DTServerLastModified" not-null="false" insert="false"/>
  </class>

  <class name="MoneyOrderFee" table="tChannelPartnerFees_MoneyOrder">
    <id name="rowguid" column="FeeMoneyOrderPK">
      <generator class="guid"/>
    </id>
    <many-to-one name="channelPartner" class="MGI.Core.Partner.Data.ChannelPartner" column="ChannelPartnerPK" />
    <property name="Fee"/>
    <property name="DTServerCreate" not-null="true" update="false"/>
    <property name="DTServerLastModified" not-null="false" insert="false"/>
  </class>

</hibernate-mapping>