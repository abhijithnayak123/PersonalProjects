<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="MGI.Core.Partner"
                   namespace="MGI.Core.Partner.Data">
  <class name="ShoppingCart" table="tShoppingCarts">
    <id name="rowguid" column="CartPK">
      <generator class="guid"/>
    </id>
    <property name="Id" generated="insert" update="false" insert="false">
      <column name="CartID" not-null="true" sql-type="bigint IDENTITY(1000000000,1)"/>
    </property>
    <property name="Active" not-null="true"/>
	<property name="Status"></property>
    <property name="DTTerminalCreate" not-null="true" update="false"/>
    <property name="DTTerminalLastModified" not-null="false" insert="false"/>
    <many-to-one name="Customer" class="Customer" column="CustomerPK" not-null="false"/>
	<set name="ShoppingCartTransactions" table="tShoppingCartTransactions" inverse="true" cascade="save-update">
		<key column="CartPK" />
		<one-to-many class="ShoppingCartTransaction"  />
	</set>
    <!--US1488 Parking Transaction Changes -->
     <property name="IsParked" not-null="true" />
    <property name="IsReferral" not-null="true"/>
		<property name="DTServerCreate" column="DTServerCreate" not-null="true" update="false"/>
		<property name="DTServerLastModified" column="DTServerLastModified" not-null="false"/>
	</class>
</hibernate-mapping>