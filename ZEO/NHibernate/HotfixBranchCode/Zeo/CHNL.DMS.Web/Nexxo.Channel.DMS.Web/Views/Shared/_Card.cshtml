@model MGI.Channel.DMS.Web.Models.PinDetails
@Html.ValidationSummary(true)
<div align="center" class="error_txt">@ViewBag.ExceptionMessage</div>
<div class="row">
   <div>@Html.LabelFor(model => model.CardNumber, new { @class="row_label_txt"})
        @*<label class="row_label_txt">@MGI.Channel.DMS.Web.App_GlobalResources.Nexxo.MemberShipCardNumber</label>*@
        @Html.TextBoxFor(model => model.CardNumber, new { @class = "disable_txt", @disabled = "disabled", autocomplete = "off" })
    </div> 
   
    <div class="card_wrap">
        @Html.ValidationMessageFor(model => model.CardNumber)
    </div>
</div>
