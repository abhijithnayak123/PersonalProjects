@model MGI.Channel.DMS.Web.Models.BaseModel ﻿
@{
	Layout = null;
	System.Collections.Hashtable htSessions = (System.Collections.Hashtable)Session["HTSessions"];
	MGI.Channel.DMS.Server.Data.AgentSession agentSession = ((MGI.Channel.DMS.Server.Data.AgentSession)(htSessions["TempSessionAgent"]));

	bool isCustomerSessionActivated = Model.customerSession != null;

	string peripheralServiceUrl = string.Empty;
	if (agentSession.Terminal != null && agentSession.Terminal.PeripheralServer != null)
	{
		if (!string.IsNullOrWhiteSpace(agentSession.Terminal.PeripheralServer.PeripheralServiceUrl))
		{
			peripheralServiceUrl = agentSession.Terminal.PeripheralServer.PeripheralServiceUrl;
		}
	}

	int isTerminalSetup = 0;
	int isPeripheralServerSetUp = 0;

	if (Session["IsTerminalSetup"] != null)
	{
		isTerminalSetup = Convert.ToInt32(Session["IsTerminalSetup"]);
	}

	if (Session["IsPeripheralServerSetUp"] != null)
	{
		isPeripheralServerSetUp = Convert.ToInt32(Session["IsPeripheralServerSetUp"]);
	}
    string isAgentSessionId = string.Empty;
    if(agentSession.SessionId!=null)
    {
        isAgentSessionId = agentSession.SessionId;
    }
}
