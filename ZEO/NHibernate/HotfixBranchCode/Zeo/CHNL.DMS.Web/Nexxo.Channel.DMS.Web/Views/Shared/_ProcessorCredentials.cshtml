@model List<MGI.Channel.DMS.Web.Models.ProcessorCrendential>
@{
	ViewBag.Title = "Enter Processor Credentials";
}
<script src="@Url.Content("~/Scripts/Common/lib/jquery.maskedinput-1.3.js")" type="text/javascript"></script>
<link rel="stylesheet" href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" />
<script src="http://code.jquery.com/jquery-1.9.1.js"></script>
<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>

<script type="text/javascript">
	$(document).ready(function () {
		$("#tabs").tabs();
	});

</script>

@foreach (var processor in Model)
{

	<div id="tabs">
		<ul>
			<li><a href="#INGO">INGO</a></li>
			<li><a href="#TSYS">TSYS</a></li>
			<li><a href="#WESTERNUNION">WESTERN UNION</a></li>
			<li><a href="#CHEXAR">CHEXAR</a></li>
		</ul>
		<div id="INGO">
			<div class="row">
				<div>
					@Html.LabelFor(model => processor.UserName)
					@Html.TextBoxFor(model => processor.UserName, processor.UserName, new { @class = "input_box", maxLength = "100" })
				</div>
				<div class="error_align_left">
					@Html.ValidationMessageFor(model => processor.UserName)
				</div>
			</div>
			<div class="row">
				<div>
					@Html.LabelFor(model => processor.Password)
					@Html.TextBoxFor(model => processor.Password, processor.Password, new { @class = "input_box", maxLength = "100", TextMode = "Password" })
				</div>
				<div class="error_align_left">
					@Html.ValidationMessageFor(model => processor.Password)
				</div>
			</div>
		</div>
		<div id="TSYS">
			<div class="row">
				<div>
					@Html.LabelFor(model => processor.ProgramID)
					@Html.TextBoxFor(model => processor.ProgramID, processor.ProgramID, new { @class = "input_box", maxLength = "100" })
				</div>
				<div class="error_align_left">
					@Html.ValidationMessageFor(model => processor.ProgramID)
				</div>
			</div>
		</div>
		<div id="WESTERNUNION">
			<div class="row">
				<div>
					@Html.LabelFor(model => processor.CounterID)
					@Html.TextBoxFor(model => processor.CounterID, processor.CounterID, new { @class = "input_box", maxLength = "100" })
				</div>
				<div class="error_align_left">
					@Html.ValidationMessageFor(model => processor.CounterID)
				</div>
			</div>
		</div>
		<div id="CHEXAR">
			<div class="row">
				<div>
					@Html.LabelFor(model => processor.CounterID)
					@Html.TextBoxFor(model => processor.CounterID, processor.CounterID, new { @class = "input_box", maxLength = "100" })
				</div>
				<div class="error_align_left">
					@Html.ValidationMessageFor(model => processor.CounterID)
				</div>
			</div>
		</div>
	</div>
}