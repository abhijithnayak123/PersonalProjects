CREATE PROCEDURE dbo.USP_IE_GetAllAccountTypes
	-- paremeters here  
AS
--  Initial Set statements  --  
SET NOCOUNT ON;
SET LOCK_TIMEOUT 30000;-- 30 seconds  

--  Variable Declarations  --  
DECLARE @procname VARCHAR(60);

--  Variable Data Assignment  --  
SET @procname = 'USP_IE_GetAllAccountTypes';

-- Body of procedure  --  
SELECT DISTINCT AccountTypeCode AS AccountType
FROM SYN_IT_AccountMaster
ORDER BY 1

SET NOCOUNT OFF;
	-- End of procedure  --  
