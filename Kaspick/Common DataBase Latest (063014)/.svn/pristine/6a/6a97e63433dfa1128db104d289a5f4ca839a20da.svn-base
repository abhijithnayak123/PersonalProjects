CREATE PROCEDURE [dbo].[USP_PV_GetValuationStatus]
AS
BEGIN
	--  Initial Set statements  --    
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds 

	-- Get all valuation status 
	SELECT StatusID
		,STATUS
	FROM TBL_PV_ValuationStatus
END
