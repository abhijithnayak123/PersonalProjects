CREATE PROCEDURE dbo.USP_IE_GetAllUniverseGroups
AS
--  Initial Set statements  --  
SET NOCOUNT ON;
SET LOCK_TIMEOUT 30000;-- 30 seconds  

-- Body of procedure  --  
SELECT GroupID
	,GroupName
	,TaxYear
	,IsOfficial
	,[Description]
	,IsDeleted
	,UserID
	,CreatedDate
	,ModifiedDate
FROM TBL_IE_Group
WHERE IsDeleted = 0
ORDER BY IsOfficial DESC
	,TaxYear DESC
	,GroupName ASC;

SET NOCOUNT OFF;
