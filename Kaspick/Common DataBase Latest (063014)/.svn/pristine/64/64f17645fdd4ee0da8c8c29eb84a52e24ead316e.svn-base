CREATE PROCEDURE [dbo].[USP_OP_GetDataImportByImportDate]
(
	@FromDate DateTime,
	@ToDate DateTime
)
AS

BEGIN
SET NOCOUNT ON

select di.* , u.Loginname as RunUserName
from TBL_OP_DataImport  di
inner join dbo.TBL_KS_User u on di.RunUserID=u.userid
WHERE  convert(varchar(10), RunDate, 110) between convert(varchar(10), @FromDate, 110) AND convert(varchar(10), @ToDate   , 110)


END
GO


