CREATE PROCEDURE dbo.[USP_PP_GetAdminPaymentProcess]
AS
BEGIN TRY
	--  Initial Set statements  --      
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds  

	-- Body of procedure  -- 
	-- Top 60 is added to fetch the last 60 processes based on ProcessStartDate     
	SELECT TOP (60) PaymentPostingProcessLogID
		,ProcessStartDateTime
		,ProcessEndDateTime
		,Status
		,FailureDescription
		,PaymentsPosted
		,PaymentsNotPosted
		,PaymentsValidated
		,PaymentsFailed
	FROM TBL_PP_PaymentPostingProcessLog
	ORDER BY ProcessStartDateTime DESC
END TRY

BEGIN CATCH
	SELECT ERROR_MESSAGE() AS ErrorMessage
END CATCH
