CREATE PROCEDURE USP_SIT_GetAllAccountTypes
	-- paremeters here    
AS
BEGIN
	--  Initial Set statements  --    
	SET ANSI_NULLS OFF;
	SET QUOTED_IDENTIFIER OFF;

	--  Variable Declarations  --    
	--  Variable Data Assignment  --    
	-- Body of procedure  --  
	SELECT LI.ListItemID
		,LI.ListItemName
	FROM TBL_ListItem LI
	INNER JOIN TBL_ListType LT ON LT.ListTypeID = LI.ListTypeID
	WHERE LT.ListTypeName = 'SUBACCOUNTING ACCOUNT TYPES';

	SET NOCOUNT OFF;
END
