CREATE PROCEDURE [dbo].[USP_PP_GetPGCalcPaymentBatch]  
 -- parameters here    
AS  
BEGIN  
 --  Initial Set statements  --    
 SET NOCOUNT ON;  
 SET LOCK_TIMEOUT 30000;  
 
  
 --  Variable Declarations  --    
 --counters    
 --select PaymentBatch,SourceFile,Description from TBL_PP_PGCalcPaymentBatch    
 SELECT PaymentBatch  
  ,SourceFile  
  ,Description  
 FROM TBL_PP_PGCalcPaymentBatch  
 ORDER BY LoadDate DESC  
  
 SET NOCOUNT OFF  
END  
 -- End of procedure  --    
GO


