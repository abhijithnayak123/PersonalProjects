CREATE PROCEDURE [dbo].[USP_EX_GetSysadminCgasetupDetail] (@CGASETUPID INT)
AS
BEGIN
	SELECT SETUP_ID
		,STATE_TYPE_ID
		,CGATYPE_TYPE_ID
		,RESERVE_TYPE_ID
		,ANNUAL_FORM_TYPE_ID
		,DAYS_FORM_DUE_AFTER_FY_OR_CY
		,ANNUAL_REPORTING_FORM_RULE
		,SUBMIT_FINANCIAL_STATEMENT_TYPE_ID
		,ANNUAL_FINANCIAL_STATEMENT_TYPE_ID
		,DAYS_STATEMENT_DUE_AFTER_FY_OR_CY
		,ANNUAL_FINANCIAL_STATEMENT_RULE
		,WEBSITE_ADDRESS
	FROM TBL_BR_CGASetup
	WHERE SETUP_ID = @CGASETUPID
END
GO

IF EXISTS (
		SELECT *
		FROM sysobjects
		WHERE type = 'P'
			AND NAME = 'USP_EX_GetSysadminCgasetupDetail'
		)
BEGIN
	PRINT 'CREATED PROCEDURE USP_EX_GetSysadminCgasetupDetail';
END