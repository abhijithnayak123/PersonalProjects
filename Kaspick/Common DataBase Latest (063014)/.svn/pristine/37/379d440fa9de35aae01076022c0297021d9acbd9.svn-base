CREATE PROCEDURE [dbo].[USP_EX_GetWftRequestFlowStatusList]
	-- paremeters here  
AS
BEGIN
	-- Body of procedure  --  
	SELECT flow_status_id
		,flow_status_name
		,flow_status_abbrev
	FROM TBL_WFT_FlowStatus
	ORDER BY flow_status_abbrev
END
GO

SET NOCOUNT OFF;

IF EXISTS (
		SELECT *
		FROM sysobjects
		WHERE type = 'P'
			AND NAME = 'USP_EX_GetWftRequestFlowStatusList'
		)
BEGIN
	PRINT 'CREATED PROCEDURE USP_EX_GetWftRequestFlowStatusList';
END