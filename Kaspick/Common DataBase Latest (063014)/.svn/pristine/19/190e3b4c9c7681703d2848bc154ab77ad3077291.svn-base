CREATE PROCEDURE [dbo].[USP_IE_ImportDelSTGHolding] 
@XMLDATA XML
AS

BEGIN
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds    

	DELETE
	FROM TBL_IE_STG_Holding
	WHERE StagingID IN (
			SELECT XmlInput.Item.value('@StagingID[1]', 'int') AS StagingID
			FROM @XMLDATA.nodes('//StgHoldingCollection/DeleteList/StgHolding') AS XmlInput(Item)
			)

	SET NOCOUNT OFF;
END
