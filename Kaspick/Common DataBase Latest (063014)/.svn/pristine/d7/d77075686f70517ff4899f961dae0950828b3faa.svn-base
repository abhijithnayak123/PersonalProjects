CREATE PROCEDURE dbo.USP_IE_GetAllManagerCodes
	-- paremeters here  
AS
BEGIN
	--  Initial Set statements  --  
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds  

	--  Variable Declarations  --  
	DECLARE @procname VARCHAR(60);

	--  Variable Data Assignment  --  
	SET @procname = 'USP_IE_GetAllManagerCodes';

	-- Body of procedure  --  
	SELECT ManagerCode
	FROM SYN_IT_AccountManagerCodes
	WHERE ActiveFlag = - 1
	ORDER BY ManagerCode;

	SET NOCOUNT OFF;
END
	-- End of procedure  --  
