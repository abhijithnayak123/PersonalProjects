CREATE PROCEDURE [dbo].[USP_PP_InsStagingPGCalcPaymentData] @CreatedBy INT
AS
SET NOCOUNT ON
SET LOCK_TIMEOUT 30000

BEGIN
	TRUNCATE TABLE dbo.TBL_PP_STG_PGCalcPaymentData

	INSERT INTO dbo.TBL_PP_STG_PGCalcPaymentData (
		PGCalcPaymentID
		,PaymentID
		,PaymentBatch
		,CustomerAccountNumber
		,ContactID
		,ContactRoleCode
		,InstructionID
		--,beneficiaryDistributionID
		,PersonCode
		,PayeeName
		,PaymentAmount
		,PaymentDate
		,SeparateCheck
		,Account1
		,Account2
		,GiftDate
		,PIFUnits
		,PIFValue
		,PIFIncome
		,TaxWithHolding
		,PeriodEndDate
		,IsDeleted
		,ModifiedBy
		,ModifiedDate
		,CreatedBy
		,CreatedDate
		)
	SELECT DISTINCT PGCalcPaymentID
		,PGCalc.PaymentID
		,PGCalc.PaymentBatch
		,PGCalc.CustomerAccountNumber
		,PGCalc.ContactID
		,PGCalc.ContactRoleCode
		,InstructionID
		--,beneficiaryDistributionID
		,PersonCode
		,PayeeName
		,PaymentAmount
		,PGCalc.PaymentDate
		,SeparateCheck
		,Account1
		,Account2
		,GiftDate
		,PIFUnits
		,PIFValue
		,PIFIncome
		,TaxWithholding
		,PeriodEndDate
		,0 AS DELETEFLAG
		,@CreatedBy AS MODIFIEDBY
		,GetDate() AS MODIFIEDDATE
		,@CreatedBy AS CREATEDBY
		,GetDate() AS CREATEDDATE
	FROM dbo.TBL_PP_PGCalcPaymentData PGCalc
	INNER JOIN dbo.TBL_PP_PendingPayment PendgPymt ON PGCalc.PaymentID = PendgPymt.PaymentID
	WHERE AccountType = 'PIF'
END
