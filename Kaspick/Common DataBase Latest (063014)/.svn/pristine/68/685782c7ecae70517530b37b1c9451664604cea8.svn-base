CREATE PROCEDURE [dbo].[USP_IE_ImportDelUserSTGTransaction] @UserID INT
AS
BEGIN
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds    
 
	DELETE FROM TBL_IE_STG_Transaction 
	--WHERE UserID = @UserID

	SET NOCOUNT OFF;
END
