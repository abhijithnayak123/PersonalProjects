CREATE PROCEDURE [dbo].[USP_RP_GetRPTClient]
AS
BEGIN
	SELECT lkupClient.ClientID AS ClientID
		,acntMgrCodes.Managercode AS BriefName
	FROM dbo.SYN_IT_AccountManagerCodes acntMgrCodes
		INNER JOIN  TBL_Lookup_Client lkupClient on lkupClient.ManagerCode = acntMgrCodes.ManagerCode
	ORDER BY BriefName ASC
END
GO

IF EXISTS (
		SELECT *
		FROM sysobjects
		WHERE type = 'P'
			AND NAME = 'USP_RP_GetRPTClient'
		)
BEGIN
	PRINT 'CREATED USP_RP_GetRPTClient';
END