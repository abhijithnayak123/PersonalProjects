CREATE PROCEDURE [dbo].[USP_EX_GetRptFasbAllInstance]  --'ACL'    
(                
 @ManagerCode VARCHAR(15)            
)                
AS                
BEGIN              
      select FASB_InstanceID as 'Value' ,InstanceLabel as 'Text'   
      from dbo.TBL_BR_FASBProfileInformation where ManagerCode =  @ManagerCode         
END      



GO
  IF EXISTS (	SELECT *
			FROM sysobjects
			WHERE type = 'P'
			AND name = 'USP_EX_GetRptFasbAllInstance') 
	BEGIN
			PRINT 'CREATED PROCEDURE USP_EX_GetRptFasbAllInstance';
	END