CREATE PROCEDURE UPS_SIT_GetAuditLog (@UserID INT)
AS
BEGIN
	SELECT [UserID]
		,[Prior_Reference_Number]
		,[ImportDateTime]
		,[IsSuccess]
		,[ErrorDetail]
	FROM TBL_SIT_AuditLog
	WHERE CONVERT(VARCHAR, ImportDateTime, 101) = CONVERT(VARCHAR, GetDate(), 101)
		AND [UserID] = @UserID
END
