CREATE PROCEDURE [dbo].[USP_TR_GetPaymentSnapshot] @EventID INT
	,@EventAccountID INT
AS
BEGIN
	--  Initial Set statements  --          
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds          

	SELECT PaymentDate
		,ScheduledAmount
		,IMlevel1Count
		,IMlevel2Count
		,AnnualPayment
	FROM TBL_TR_EventACCOUNT
	WHERE EventID = @EventID
		AND EventAccountID = @EventAccountID

	SET NOCOUNT OFF;
END
GO


