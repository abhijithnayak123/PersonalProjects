CREATE PROCEDURE dbo.USP_PP_GetSystemAdministration
	-- paremeters here            
AS
--  Initial Set statements  --            
--  SET NOCOUNT ON;            
SET LOCK_TIMEOUT 30000;-- 30 seconds                        
	-- Body of procedure  --            

SELECT MinimumCheckPostingDays
	,MinimumNonCheckPostingDays
	,PaymentProcessStartTime
	,WireThresholdAmount
	,MinimumWirePostingDays

FROM TBL_PP_SystemAdministration
	-- End of procedure  --     
