CREATE PROCEDURE [dbo].[USP_PP_GetValidationEngineBeneficiaryUnsupportedAccountType]            
-- paremeters here                                
@UserID INT,
@Type Bit =1--auto 

AS                                
	BEGIN TRY        
        
		--  Initial Set statements  --        
		SET NOCOUNT ON;        
		SET LOCK_TIMEOUT                30000;   -- 30 seconds  
		
		
		---Declare Variable
		DECLARE @RuleID INT  
		                
		---Assigning value to Variable
		SELECT @RuleID = RuleID
		FROM TBL_PP_ValidationRule
		WHERE RuleName = 'Unsupported Beneficiary Account Type'
			AND RuleCategory = 'Beneficiary payment'            
            
		-- Body of procedure  --            
		-- Return Data           
		SELECT PaymentType
			,StgData.ScheduleID
			,@RuleID AS RuleID
			,'AccountType' AS Attribute
			,AccountTypeCode AS ActualValue
		FROM TBL_PP_STG_InnoTrustValidationEngineInput StgData
		WHERE PaymentType = 'Beneficiarypayment'
			AND UserID = @UserID
          
END TRY          
BEGIN CATCH

	SELECT ERROR_MESSAGE() AS ErrorMessage
	
END CATCH 

SET NOCOUNT OFF;