CREATE PROCEDURE [dbo].[USP_TR_GetAssetClassList]
AS
BEGIN
	SELECT AssetClassName
		,AssetClassCategory
		,AssetClassType
		,ClassDisplayOrder
		,AssetClassDescription
		,AssetClassScaler
		,AssetClassEquivalent
	FROM TBL_TR_AssetClassList
END
