CREATE PROCEDURE [dbo].[USP_PV_GetPendingInnoTrustTransaction] (@ValuationXML XML)
AS
BEGIN
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds        

	SELECT TransactionID
		,ValuationID
		,MatchKey
		,MatchStatus
		,MatchComment
		,GroupID
		,TransactionCode
		,[Security]
		,TradeDate
		,Quantity
		,TradeAmount
		,Comment
	FROM TBL_PV_ValuationInnotrustTransaction ValTran
	WHERE ValTran.ValuationID IN (
			SELECT XMLDATA.item.value('@ValuationID[1]', 'int') AS ValuationID
			FROM @ValuationXML.nodes('//ValuationCollection/InsertList/Valuation') AS XMLDATA(item)
			)
		AND LTRIM(RTRIM(ValTran.MatchStatus)) = '';
END

SET NOCOUNT OFF;

