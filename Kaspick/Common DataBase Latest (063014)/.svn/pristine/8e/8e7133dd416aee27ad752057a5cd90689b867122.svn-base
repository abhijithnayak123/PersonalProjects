CREATE PROCEDURE [dbo].[USP_PP_GetValidationEngineBeneficiarySchwabMoneylinkData]
	-- paremeters here                                
	@UserID INT
	,@Type BIT = 1 --auto 
AS
BEGIN TRY
	--  Initial Set statements  --        
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds  

	---Declare Variable
	DECLARE @RuleID INT

	---Assigning value to Variable
	SELECT @RuleID = RuleID
	FROM TBL_PP_ValidationRule
	WHERE RuleName = 'Schwab Moneylink data not sent'
		AND RuleCategory = 'Beneficiary payment'

	----Return Data 
	SELECT PaymentType
		,ScheduleID
		,@RuleID AS RuleID
		,'MoneyLinkSetUpCreated' AS Attribute
		,CASE 
			WHEN ISNULL(ExtractStatus, '') = ''
				AND AuthorizedFlag = 'TRUE'
				AND ExtractFlag = 'TRUE'
				THEN 'True'
			ELSE 'False'
			END AS ActualValue
	FROM TBL_PP_STG_InnoTrustValidationEngineInput
	WHERE PaymentType IN (
			'Beneficiarypayment'
			,'PGCalcImport'
			)
		AND UserId = @UserID
END TRY

BEGIN CATCH
	SELECT ERROR_MESSAGE() AS ErrorMessage
END CATCH
SET NOCOUNT OFF;