CREATE PROCEDURE [dbo].[USP_EX_GetUserApplication]    
 -- paremeters here    
 @USER_ID int     
AS     
--  Initial Set statements  --    
SET NOCOUNT ON;    
Select APPLICATIONID,APPLICATIONNAME     
From TBL_KS_Application     
Where APPLICATIONID in (Select APPLICATIONID From TBL_KS_UserApplication Where   
UserId=@USER_ID)   
GO
    
SET NOCOUNT OFF;

  IF EXISTS (	SELECT *
			FROM sysobjects
			WHERE type = 'P'
			AND name = 'USP_EX_GetUserApplication') 
	BEGIN
			PRINT 'CREATED PROCEDURE USP_EX_GetUserApplication';
	END   
    