CREATE PROCEDURE [dbo].[USP_EIS_RPT_BR_VE_DeliverableItemsToBeValidated_SelProc]           
AS      
BEGIN
	--  Initial SET statements  --      
	--SET NOCOUNT ON;      
	--SET LOCK_TIMEOUT                30000;   -- 30 seconds      
	--SET TRANSACTION ISOLATION LEVEL SNAPSHOT;      

	--  Variable Declarations  --      
	DECLARE @procname    VARCHAR(60);      
	DECLARE @ErrorDesc     VARCHAR(1000);      
	DECLARE @ErrorNumber     INT;
	-- Variables used for error handling - uncomment if needed      
	DECLARE @val1      VARCHAR(30);      
	DECLARE @val2      VARCHAR(30);      

	--  Temp tables, Cursors, Table Variables  --      

	--  Variable Data Assignment  --      
	SET @procname = 'USP_EIS_RPT_BR_VE_DeliverableItemsToBeValidated_SelProc';      

	-- Body of procedure  --      
	BEGIN TRY
		SELECT 
		 DI.DeliverableItemID,
		 DI.DeliverableQueueID,
		 DI.Clientid,
		 DI.ClientEmployeeID,
		 DI.AccountID,
		 DI.TrustparticipantID,
		 DI.BeneficiaryID,
		 DI.DonorID,
		 DI.GiftKey,
		 DT.DeliverableFrequency
		FROM TBL_EIS_DT_Deliverable_Items DI
		JOIN TBL_EIS_DT_Deliverable_Queue DQ ON DI.DeliverableQueueID = DQ.DeliverableQueueID 
		JOIN TBL_EIS_DT_ItemMethodStatus IMS ON DI.DeliverableItemID = IMS.DeliverableItemID
		INNER JOIN DLV_Deliverable DD on DD.DeliverableID = DQ.DeliverableID
		JOIN TBL_EIS_DT_Deliverable_Methods DM ON IMS.DeliveryMethodID = DM.DeliveryMethodID         --ET #11231
													AND DQ.DeliverableID= DM.DeliverableID
													AND DM.IsPrimary = 1   
		JOIN TBL_EIS_DT_DeliverableProcess_Statuses DIS ON IMS.DeliverableItemStatusID = DIS.DeliverableItemStatusID 
														--AND DIS.DeliverableTypeID = DQ.DeliverableTypeID
														AND DIS.DeliverableProcessID = DD.DeliverableProcessID
		JOIN TBL_EIS_DT_Queue_Statuses QS ON DQ.QueueStatusID = QS.QueueStatusID 
		JOIN V_EIS_DT_Deliverable DT ON DQ.DeliverableID = DT.DeliverableID
		--Update to Exclude OnHold reports Begin
		JOIN TBL_EIS_DT_Deliverable_Type_Attributes DTA ON DQ.DeliverableID = DTA.DeliverableID
														   AND DTA.AttributePromptName='Hold'
        -- New Kaspick DB table name replaced here. 
        LEFT JOIN TBL_DLV_DeliverableItemDeliverableTypeAttribute DIDTA ON DI.DeliverableItemID = DIDTA.DeliverableItemID 
                                                           AND DTA.DeliverableTypeAttributeID = DIDTA.DeliverableTypeAttributeID       
        --Update to Exclude OnHold reports End
		WHERE DIS.StatusName in ('scheduled','Error')
		AND QS.StatusName = 'Open'
        AND LTRIM(RTRIM(ISNULL(DIDTA.AttributeValue,''))) <> '1'  --Update to Exclude OnHold reports 
	END TRY      
	BEGIN CATCH      
   		SET @ErrorDesc = ERROR_MESSAGE();      
		SET @ErrorNumber = ERROR_NUMBER();      
		SET @val1 = '';      
		SET @val2 = '';      
		  
		EXEC dbo.spSYS_ErrorHandler @codename = @procname,      
		@errmsg = @ErrorDesc,       
		@errnbr = @ErrorNumber,      
		@val1 = '',       
		@val1str = 'USP_EIS_RPT_BR_VE_DeliverableItemsToBeValidated_SelProc: Cannot Select.',       
		@val2 = '',       
		@val2str = '';      
	END CATCH      
	-- End of procedure  --      
END
GO

