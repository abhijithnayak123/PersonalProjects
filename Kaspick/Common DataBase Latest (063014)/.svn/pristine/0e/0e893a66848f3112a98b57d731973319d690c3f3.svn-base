CREATE PROCEDURE USP_EX_GetRptFasbRateofReturnType   
AS  
BEGIN  
SELECT DISTINCT TrstTypRtRtrnTypMp.RATEOFRETURNTYPE AS LIST_ITEM_ID  
            ,LstItm.ListItemName  
            ,TrstTypRtRtrnTypMp.LabelID  
      FROM 
      
      TBL_BR_FASBTrustTypeRateofReturnTypeMap TrstTypRtRtrnTypMp  
      INNER JOIN TBL_ListItem LstItm 
      ON 
      TrstTypRtRtrnTypMp.RATEOFRETURNTYPE = LstItm.ListItemID  
END  
 GO
  IF EXISTS (	SELECT *
			FROM sysobjects
			WHERE type = 'P'
			AND name = 'USP_EX_GetRptFasbRateofReturnType') 
	BEGIN
			PRINT 'CREATED PROCEDURE USP_EX_GetRptFasbRateofReturnType';
	END  
