CREATE PROCEDURE [dbo].[USP_IE_GetAssetYield]
	-- paremeters here  
AS
BEGIN
	--  Initial Set statements  --  
	--SET NOCOUNT ON;  
	SET LOCK_TIMEOUT 30000;-- 30 seconds  

	-- Body of procedure  --  
	SELECT PayDate
		,SecuritySymbol
		,CUSIP
		,EstimatedDividend
		,ActualDividend
		,RecordDate
		,XDate
		,EstimatedSTG
		,EstimatedLTG
		,ActualSTG
		,ActualLTG
		,QualifiedDividend
		,ConfirmedRecordDate
	FROM TBL_IE_AssetYield
	ORDER BY PayDate
		,SecuritySymbol
END
	-- End of procedure  --  
