CREATE PROCEDURE [dbo].[USP_IE_ReviewGetMyClient] -- paremeters here        
	@UserID INT
AS
BEGIN
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds    

	-- Body of procedure  --  
	SELECT DISTINCT STGEst.CustomerAccountNumber
		,STGEst.ManagerCode
	FROM TBL_IE_STG_Estimate STGEst
	INNER JOIN VW_KS_SysAdminKCoStaffPaging StfPgng
		ON StfPgng.ManagerCode = STGEst.ManagerCode
	WHERE StfPgng.UserID = @UserID
	ORDER BY STGEst.CustomerAccountNumber

	SET NOCOUNT OFF;
END
