CREATE PROCEDURE [dbo].[USP_IE_ImportGetGroupAccountType]
	-- paremeters here
	@GroupID INT
AS
BEGIN
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds    

	--	Body of procedure  --
	SELECT DISTINCT AccountType
	FROM TBL_IE_STG_Estimate
	WHERE GroupID = @GroupID
		AND IsDeleted = 0
	ORDER BY AccountType;

	SET NOCOUNT OFF;
END
