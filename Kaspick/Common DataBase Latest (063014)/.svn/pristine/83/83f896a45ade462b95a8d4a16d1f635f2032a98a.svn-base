CREATE PROCEDURE [dbo].[USP_PP_GetActivityConsoleProjectedCashPayout]     
		@TaxYear				INT,    
		@CustomerAccountNumber	VARCHAR(14),  
		@ReturnStatus			Money OUTPUT      
AS       
      
   	--  Initial Set statements  --    
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds     
      
   
-- Body of procedure  --      
BEGIN      
    
   
	SELECT @ReturnStatus			= EstAnnualPayout 
		FROM TBL_PP_AnnualAccountPayoutInfo 
	WHERE	PayoutYear				= @TaxYear 
	AND		CustomerAccountNumber	= @CustomerAccountNumber
  
END 
                  
