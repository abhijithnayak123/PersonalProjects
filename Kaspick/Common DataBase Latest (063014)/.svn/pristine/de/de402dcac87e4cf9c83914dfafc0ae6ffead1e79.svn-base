CREATE PROCEDURE [dbo].[USP_PP_ReportGetManagerCodeParameter]
	-- paremeters here  
AS
BEGIN
	--  Initial Set statements  --  
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;

	--  Variable Declarations  --  
	SELECT  AccMgr.ManagerCode
		,CntMst.ContactName AS ManagerDescription
	FROM SYN_IT_AccountManagerCodes AccMgr
	INNER JOIN SYN_IT_ContactMaster CntMst
		ON AccMgr.ContactID = CntMst.ContactID
	ORDER BY AccMgr.ManagerCode

	SET NOCOUNT OFF
END
	-- End of procedure  --  
