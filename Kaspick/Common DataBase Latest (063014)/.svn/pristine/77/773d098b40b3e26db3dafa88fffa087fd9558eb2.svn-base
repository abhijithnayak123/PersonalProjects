  --For Client Reporting Indices Management       
  SET @TEMP = NULL    
    
  EXEC USP_EX_GetRptReportIndicesManagementDetail @CLIENTID = @ENTITYID    
   ,@INDICESXML = @INDICES OUTPUT    
