
IF EXISTS (SELECT *
           FROM   sysobjects 
           WHERE  type = 'P'
                  AND name = 'USP_EX_GetPolicyReportingDTL')
    BEGIN
        DROP PROCEDURE USP_EX_GetPolicyReportingDTL;
        PRINT 'DROPPED USP_EX_GetPolicyReportingDTL';
    END
GO


      
