CREATE PROCEDURE [dbo].[USP_IE_GetAllWarning]
	-- paremeters here  
AS
--  Initial Set statements  --  
--SET NOCOUNT ON;  
SET LOCK_TIMEOUT 30000;-- 30 seconds  

-- Body of procedure  --  
SELECT DiagnosticField AS [Warning]
	,DiagnosticID AS [WarningID]
FROM TBL_IE_Diagnostic
WHERE DiagnosticType = 'Flag'
ORDER BY [Warning]
	-- End of procedure  --  
