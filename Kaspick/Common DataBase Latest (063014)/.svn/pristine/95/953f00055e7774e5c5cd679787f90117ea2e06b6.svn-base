IF EXISTS (
		SELECT *
		FROM sysobjects
		WHERE type = 'P'
			AND NAME = 'USP_EX_UpdPolicyItemCascade'
		)
BEGIN
	PRINT 'CREATED PROCEDURE USP_EX_UpdPolicyItemCascade';
END