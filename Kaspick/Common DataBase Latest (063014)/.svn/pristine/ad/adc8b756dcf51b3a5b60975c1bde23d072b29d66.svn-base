CREATE PROCEDURE [dbo].[USP_TR_GetAllEventStatus]
AS
BEGIN
	SET NOCOUNT ON

	SELECT DISTINCT EventStatus
		,EventStatusID
		,StatusTargetID
	FROM TBL_TR_EventStatus
	WHERE EventStatus <> 'Review2'
	ORDER BY EventStatusID ASC

	SET NOCOUNT OFF
END
GO


