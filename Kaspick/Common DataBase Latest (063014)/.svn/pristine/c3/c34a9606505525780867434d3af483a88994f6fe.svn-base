** --------  --------	 ------    -------------------------------------- 
** 
***
*******************************************************************************                      
** Copyright (C) <CopyrightYear,,Year> Kaspick & Company, All Rights Reserved                      
** COMPANY CONFIDENTIAL -- NOT FOR DISTRIBUTION                      
*******************************************************************************/
CREATE PROCEDURE [dbo].[USP_KCO_TAGRequest] (
	@SubmittedDateFrom DATETIME
	,@SubmittedDateTo DATETIME
	)
AS
BEGIN
	--  Initial Set statements  --    
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds 

	SELECT WFTReq.request_id 'Request ID'
		,CONVERT(CHAR(20), WFTReq.submitted_datetime, 22) 'Submitted DateTime'
		,WFTReq.ManagerCode 'Manager Code'
		,FrmCtrl.FlowTitle 'Flow Title'
		,ListItm.ListItemName 'Status'
	FROM TBL_WFT_Request WFTReq
	INNER JOIN TBL_WFT_ML_FrmCtrl FrmCtrl
		ON WFTReq.FormID = FrmCtrl.FormID
	INNER JOIN TBL_ListItem ListItm
		ON ListItm.ListItemID = WFTReq.request_status_id
	WHERE WFTReq.submitted_datetime >= @SubmittedDateFrom
		AND WFTReq.submitted_datetime <= @SubmittedDateTo
	ORDER BY WFTReq.Request_Id

	SET NOCOUNT OFF;
END
