CREATE PROCEDURE [dbo].[USP_EX_GetWftRequestTypeList]
	-- paremeters here  
AS
BEGIN
	SELECT WftFrmCtrlExtn.FormID
		,WftFrmCtrlExtn.form_abbrev
	FROM TBL_WFT_ML_FrmCtrl WftFrmCtrl
	INNER JOIN TBL_WFT_ML_FrmCtrlExtn WftFrmCtrlExtn ON WftFrmCtrl.formid = WftFrmCtrlExtn.formid
	WHERE active = 1
		AND WftFrmCtrlExtn.FormID NOT IN (
			148
			,150
			,151
			) --excluding the forms- "Authorization For Direct Deposit","Fax Cover Sheet"&"Third Party Acct Info Auth"  
	ORDER BY WftFrmCtrlExtn.form_abbrev
END
GO

SET NOCOUNT OFF;

IF EXISTS (
		SELECT *
		FROM sysobjects
		WHERE type = 'P'
			AND NAME = 'USP_EX_GetWftRequestTypeList'
		)
BEGIN
	PRINT 'CREATED PROCEDURE USP_EX_GetWftRequestTypeList';
END