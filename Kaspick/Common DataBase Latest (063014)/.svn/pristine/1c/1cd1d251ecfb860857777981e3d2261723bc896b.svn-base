CREATE PROCEDURE [dbo].[USP_OP_GetMissingSymbolsForRunID] 
	@RunID int
AS       
	
BEGIN

select sym.*,  status.StatusName
from TBL_OP_CustodialDataImportMissingSymbols sym
Inner join TBL_OP_MissingSymbolsStatus status on sym.MissingStatus=status.StatusID
 WHERE sym.RunID = @RunID

End
GO


