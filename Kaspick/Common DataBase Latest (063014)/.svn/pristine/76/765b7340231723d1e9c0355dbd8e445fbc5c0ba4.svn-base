	INNER JOIN TBL_PP_ValidationRule ValRule
		ON ValRes.RuleID = ValRule.RuleID
	INNER JOIN #TmpResult TmpRes
		ON ValRes.PaymentScheduleID = TmpRes.PGcalcPaymentID
