		,ACHProcessStatus
		,ClearDate
		,Error
		,Warning
		,HoldStatus
		,TaxYear
		,PayerName
		,Backbuilt
		,Memo
		,Comment
		,PostedBy
		,ScheduleID
		,PaymentID
		,AccountType
		,PGCalcPaymentID
		,Recurrent
		,RecordVersion
		,AllScheduleID
		,ValidationPaymentType
		,FlipDate
		,FlipType
		,Withholding
		,WireSubmitted
		,WireSubmittedBy
