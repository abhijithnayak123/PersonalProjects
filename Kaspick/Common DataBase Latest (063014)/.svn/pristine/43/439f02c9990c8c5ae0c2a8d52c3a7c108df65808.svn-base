CREATE PROCEDURE [dbo].[USP_PP_GetSchedulePaymentGroup]
	-- parameters here      
AS
--  Initial Set statements  --      
SET NOCOUNT ON;
SET LOCK_TIMEOUT 30000;-- 30 seconds      
	--SET TRANSACTION ISOLATION LEVEL SNAPSHOT;

-- Body of procedure  --      
SELECT PaymentStagingGroupID
	,PaymentStagingGroupName
	,TaxYear
FROM TBL_IE_PaymentStagingGroup
WHERE IsOfficial <> 0
ORDER BY RunDate DESC

SET NOCOUNT OFF;
	-- End of procedure  --     
GO


