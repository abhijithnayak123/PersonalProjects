CREATE PROCEDURE [dbo].[USP_OP_GetAllUserAuditFileFolders] 

AS     
BEGIN
	--  Initial Set statements  --    
	SET NOCOUNT ON;    
	SET LOCK_TIMEOUT                30000;   -- 30 seconds    
	--SET TRANSACTION ISOLATION LEVEL SNAPSHOT;    

	
select fl.UserID,u.LoginName, AuditFileFolder, ExceptionFileFolder, u.IsActive
from 
TBL_OP_UserAuditFileFolder fl
inner join dbo.TBL_KS_User u on fl.userid=u.UserID

END 
GO


