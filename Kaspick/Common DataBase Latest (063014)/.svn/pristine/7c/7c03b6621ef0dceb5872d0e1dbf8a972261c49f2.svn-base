--This script is to update the beneficiary taxcode value in the TBL_KS_ApplicationInnoTrustKey table.
SET NOCOUNT ON
DECLARE @ApplicationID INT

SELECT @ApplicationID = ApplicationID
FROM TBL_KS_Application
WHERE ApplicationName = 'Period End Valuation Automation'

UPDATE TBL_KS_ApplicationInnoTrustKey set [Value] = '052,570,737,691,697,595,596,601,602' where [ApplicationID] = @ApplicationID and [Key] = 'BenePayDisTaxCode'