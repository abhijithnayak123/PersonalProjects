CREATE PROCEDURE [dbo].[USP_EX_GetTradeGroupData] (
	@Mode VARCHAR(10)
	,@GroupID BIGINT
	,@AccountID CHAR(14)
	)
AS
BEGIN
	--  Variable Declarations  --              
	DECLARE @procname VARCHAR(60);
	DECLARE @ErrorMessage VARCHAR(1000);
	DECLARE @ErrorNumber INT;
	DECLARE @ClientID VARCHAR(15);
	-- Variables used for error handling - uncomment if needed              
	DECLARE @val1 VARCHAR(30);
	DECLARE @val2 VARCHAR(30);

	--  Variable Data Assignment  --              
	SET @procname = 'USP_EX_GetTradeGroupData';

	-- Body of procedure  --              
	BEGIN TRY
		SELECT GroupName
		FROM TBL_TR_TradeGroup
		WHERE GroupID = @GroupID

		SELECT @ClientID = ClientID
		FROM VW_EX_Account
		WHERE AccountID = @AccountID;

		SELECT AdventID
		FROM VW_EX_Account
		WHERE ClientID = @ClientID
			AND AdventID NOT IN (
				SELECT CustomerAccountNumber
				FROM TBL_TR_TradeGroupAccount
				)
		ORDER BY AdventID

		SELECT CustomerAccountNumber AS AdventID
		FROM TBL_TR_TradeGroupAccount
		WHERE GroupID = @GroupID
	END TRY

	BEGIN CATCH
		SET @ErrorMessage = ERROR_MESSAGE();
		SET @ErrorNumber = ERROR_NUMBER();
		SET @val1 = '';
		SET @val2 = '';

		EXEC dbo.USP_EX_SYSErrorHandler @codename = @procname
			,@ErrorMessage = @ErrorMessage
			,@ErrorNumber = @ErrorNumber
			,@val1 = ''
			,@val1str = 'USP_EX_GetTradeGroupData: Cannot Select.'
			,@val2 = ''
			,@val2str = '';
	END CATCH
		-- End of procedure  --              
END
GO

IF EXISTS (
		SELECT *
		FROM sysobjects
		WHERE type = 'P'
			AND NAME = 'USP_EX_GetTradeGroupData'
		)
BEGIN
	PRINT 'CREATED PROCEDURE USP_EX_GetTradeGroupData';
END