Create PROCEDURE USP_EX_GetRptFasbListItem      
@LIST_TYPE_NAME varchar(100)      
AS         
select LstItm.LISTITEMID, LstItm.LISTITEMNAME   
from TBL_ListItem LstItm    
inner join TBL_ListType LstTyp on LstTyp.ListTypeID = LstItm.ListTypeID    
where LstTyp.LISTTYPENAME = @LIST_TYPE_NAME    
order by LstItm.LISTITEMID    
  
  

GO
  IF EXISTS (	SELECT *
			FROM sysobjects
			WHERE type = 'P'
			AND name = 'USP_EX_GetRptFasbListItem') 
	BEGIN
			PRINT 'CREATED PROCEDURE USP_EX_GetRptFasbListItem';
	END