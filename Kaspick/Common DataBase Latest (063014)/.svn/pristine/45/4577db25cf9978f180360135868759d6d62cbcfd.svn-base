CREATE PROCEDURE [dbo].[USP_EX_GetRptFasbAdventID]    
   
@ManagerCode VARCHAR(15)    
AS        
begin    
    
 select distinct CustomerAccountNumber from   
 VW_EX_TRUSTPARTICIPANTTYPE  where ClientID=@ManagerCode and AccountType not in ('CGA','PIF')    
 order by CustomerAccountNumber     
    
end     



GO
  IF EXISTS (	SELECT *
			FROM sysobjects
			WHERE type = 'P'
			AND name = 'USP_EX_GetRptFasbAdventID') 
	BEGIN
			PRINT 'CREATED PROCEDURE USP_EX_GetRptFasbAdventID';
	END

  