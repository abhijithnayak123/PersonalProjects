CREATE PROCEDURE [dbo].[USP_PP_GetScheduleIncomeEstimationGroup]
	-- parameters here    
AS
--  Initial Set statements  --    
SET NOCOUNT ON;
SET LOCK_TIMEOUT 30000;-- 30 seconds    
--SET TRANSACTION ISOLATION LEVEL SNAPSHOT;

-- Body of procedure  --    
SELECT GroupID AS PaymentStagingGroupID
	,GroupName AS PaymentStagingGroupName
	,TaxYear
FROM TBL_IE_Group 
WHERE TaxYear = year(getdate())
	AND IsOfficial = 1
	AND IsDeleted = 0;

SET NOCOUNT OFF;
	-- End of procedure  --    
GO


