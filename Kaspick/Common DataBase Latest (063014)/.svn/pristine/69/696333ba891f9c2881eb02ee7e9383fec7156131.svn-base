CREATE PROCEDURE dbo.USP_IE_GetAllStatus
	-- paremeters here  
AS
BEGIN
	--  Initial Set statements  --  
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds  

	-- Body of procedure  --  
	SELECT StatusID
		,[Status]
	FROM TBL_IE_Status
	ORDER BY [Status];

	SET NOCOUNT OFF;
END
	-- End of procedure  --  
