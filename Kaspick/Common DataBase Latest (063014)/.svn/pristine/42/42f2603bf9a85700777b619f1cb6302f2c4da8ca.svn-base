CREATE PROCEDURE [dbo].[USP_PP_InsArchCGAPaymentDetail]
AS
SET NOCOUNT ON

INSERT INTO dbo.TBL_PP_ARCH_CGAPaymentDetail (
	PaymentID
	,Account1
	,PaymentAmount
	,PaymentStatus
	,ProcessedByMiddleWare
	,ProcessedDateMiddleWare
	,IsPrintedOnCheck
	,PrintedDates
	,IsDeleted
	,ModifiedBy
	,ModifiedDate
	,CreatedBy
	,CreatedDate
	)
SELECT PaymentID
	,Account1
	,PaymentAmount
	,'P' AS PaymentStatus
	,1 AS ProcessedByMiddleWare
	,CreatedDate AS ProcessedDateMiddleWare
	,1 AS IsPrintedOnCheck
	,CreatedDate AS PrintedDates
	,IsDeleted
	,ModifiedBy
	,ModifiedDate
	,CreatedBy
	,CreatedDate
FROM TBL_PP_STG_CGAPaymentDetail
ORDER BY PersonCode
	,GiftDate DESC
GO


