	--	,CreatedBy
	--	,MatchType
	--	,GiftWrapPaymentReviewDate
	--	,GiftWrapPaymentReviewedBy
	--	,DeletedUserId
	--	)
	--SELECT @User
	--	,GETDATE()
	--	,'U'
	--	,'LOGIN_NAME->' + SYSTEM_USER + ',SYSTEM_ID->' + HOST_ID() + ',HOST_NAME->' + HOST_NAME() + ',USER->' + USER
	--	,PgCalc.MergedPGCalcPaymentID
	--	,PgCalc.PaymentBatch
	--	,PgCalc.ManagerCode
	--	,PgCalc.CustomerAccountNumber
	--	,PgCalc.PersonCode
	--	,PgCalc.PayeeName
