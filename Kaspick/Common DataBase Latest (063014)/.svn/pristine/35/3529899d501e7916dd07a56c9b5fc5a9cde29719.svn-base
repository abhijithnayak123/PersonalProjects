CREATE PROCEDURE USP_SIT_InsertInterfaceFileGenerationLog (
	@valuationID INT
	,@fileType VARCHAR(100)
	,@fileGenerationDateTime DATETIME
	,@fileGenerationUserID INT
	,@comment VARCHAR(256)
	)
AS
BEGIN
	DECLARE @fileTypeID INT

	SELECT @fileTypeID = FileTypeID
	FROM TBL_SIT_FileType
	WHERE FileTypeName = @fileType

	INSERT INTO TBL_SIT_InterfaceFileGenerationLog (
		ValuationID
		,FileTypeID
		,FileGenerationDateTime
		,FileGenerationUserID
		,Comment
		)
	VALUES (
		@valuationID
		,@fileTypeID
		,@fileGenerationDateTime
		,@fileGenerationUserID
		,@comment
		)
END