CREATE PROCEDURE [dbo].[USP_TR_GetAllBroker]
AS
BEGIN
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds    

	SELECT BrokerID
		,BrokerName
	FROM TBL_TR_Broker
END
GO


