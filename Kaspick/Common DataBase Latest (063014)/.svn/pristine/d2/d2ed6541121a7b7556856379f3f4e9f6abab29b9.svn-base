CREATE PROCEDURE [dbo].[USP_PP_InsPDFServiceProcessLog] @PaymentProcessID INT
	,@ProcessStartDateTime [datetime]
	,@ProcessEndDateTime [datetime]
	,@NoOfPDFsGenerated [INT]
	,@Status [varchar] (100)
	,@FailureDescription [varchar] (4000)
AS
BEGIN TRY
	--  Initial Set statements  --        
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000 -- 30 seconds   

	BEGIN TRANSACTION

	INSERT INTO TBL_PP_WirePDFProcessLog (
		PaymentProcessID
		,ProcessStartDateTime
		,ProcessEndDateTime
		,NoOfPDFsGenerated
		,STATUS
		,FailureDescription
		)
	VALUES (
		@PaymentProcessID
		,@ProcessStartDateTime
		,@ProcessEndDateTime
		,@NoOfPDFsGenerated
		,@Status
		,@FailureDescription
		)

	COMMIT TRANSACTION
	SET NOCOUNT OFF;
END TRY

BEGIN CATCH
	ROLLBACK TRANSACTION

	DECLARE @ErrorMessage NVARCHAR(4000);
	DECLARE @ErrorSeverity INT;
	DECLARE @ErrorState INT;

	SELECT @ErrorMessage = ERROR_MESSAGE()
		,@ErrorSeverity = ERROR_SEVERITY()
		,@ErrorState = ERROR_STATE();

	RAISERROR (
			@ErrorMessage
			,-- Message text.
			@ErrorSeverity
			,-- Severity.
			@ErrorState -- State.
			);

	PRINT N'The transaction is in an uncommittable state. ' + 'Rolling back transaction.'
END CATCH
