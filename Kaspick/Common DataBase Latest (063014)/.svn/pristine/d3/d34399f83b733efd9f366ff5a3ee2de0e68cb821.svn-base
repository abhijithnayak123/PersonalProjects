CREATE PROCEDURE [dbo].[USP_RP_FASB_GetListItem]
	@LIST_TYPE_NAME VARCHAR(100)
AS
BEGIN
	SELECT
		LI.ListItemID AS 'LIST_ITEM_ID', LI.ListItemName AS 'LIST_ITEM_NAME'
	FROM
		TBL_ListItem LI
	INNER JOIN
		TBL_ListType LT
	ON
		LT.ListTypeID = LI.ListTypeID
	WHERE
		LT.ListTypeName = @LIST_TYPE_NAME
	ORDER BY
		LI.ListItemID
END
GO
