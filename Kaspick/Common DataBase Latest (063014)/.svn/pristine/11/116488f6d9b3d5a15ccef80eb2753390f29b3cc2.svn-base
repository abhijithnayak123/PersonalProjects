CREATE PROCEDURE [dbo].[USP_EIS_RPT_BR_VE_DQ_BeneActive_SelProc]
	-- paremeters here        
	@XMLDeliverableItems XML
AS
BEGIN
	--  Initial Set statements  --        
	--SET NOCOUNT ON;        
	--SET LOCK_TIMEOUT                30000;   -- 30 seconds        
	--SET TRANSACTION ISOLATION LEVEL SNAPSHOT;        
	--  Variable Declarations  --        
	DECLARE @procname VARCHAR(60);
	DECLARE @ErrorMessage VARCHAR(1000);
	DECLARE @ErrorNumber INT;
	-- Variables used for error handling - uncomment if needed        
	DECLARE @val1 VARCHAR(30);
	DECLARE @val2 VARCHAR(30);
	--  Temp tables, Cursors, Table Variables  --        
	DECLARE @DeliverableItemData TABLE (
		DeliverableItemID INT
		,AccountID INT
		,Participant_Type VARCHAR(50)
		);

	--  Variable Data Assignment  --        
	SET @procname = 'USP_EIS_RPT_BR_VE_DQ_BeneActive_SelProc';

	-- Body of procedure  --        
	BEGIN TRY
		INSERT INTO @DeliverableItemData
		SELECT XMLdoc.DeliverableItemData.value('@DeliverableItemID[1]', 'int') AS DeliverableItemID
			,XMLdoc.DeliverableItemData.value('@AccountID[1]', 'int') AS AccountID
			,TPT.Participant_Type
		FROM @XMLDeliverableItems.nodes('//DeliverableItemsDataToValidateCollection/SelectList/DeliverableItemsDataToValidate') AS XMLdoc(DeliverableItemData)
		INNER JOIN V_EIS_EX_TRUSTPARTICIPANT_TYPE TPT ON TPT.AccountID = XMLdoc.DeliverableItemData.value('@AccountID[1]', 'int')
		INNER JOIN TBL_EIS_DT_DELIVERABLE_ITEMS DI ON TPT.ACCOUNTID = DI.ACCOUNTID
			AND DI.DELIVERABLEITEMID = XMLdoc.DeliverableItemData.value('@DeliverableItemID[1]', 'int')
			AND DI.BeneficiaryID = TPT.BeneficiaryID
		WHERE (TPT.ExpireDate IS NOT NULL)
			AND TPT.Participant_Type IN (
				'Donor Active/Beneficiary Active'
				,'Beneficiary Active'
				)

		SELECT DeliverableItemID
			,(
				SELECT ValidationRuleID
				FROM TBL_DLV_ValidationRule -- changed the Kaspick DB table as StagingDB table is missing
				WHERE RuleName = 'Participant Active, Death Populated'
				) AS RuleID
			,'ParticipantType' AS Attribute
			,Participant_Type AS ActualValue
			,'' AS DisplayMessage
		FROM @DeliverableItemData
		GROUP BY DeliverableItemID
			,Participant_Type
			--Select * from @DeliverableItemData
	END TRY

	BEGIN CATCH
		SET @ErrorMessage = ERROR_MESSAGE();
		SET @ErrorNumber = ERROR_NUMBER();
		SET @val1 = '';
		SET @val2 = '';

		EXEC dbo.spSYS_ErrorHandler @codename = @procname
			,@errmsg = @ErrorMessage
			,@errnbr = @ErrorNumber
			,@val1 = ''
			,@val1str = 'USP_EIS_RPT_BR_VE_DQ_BeneActive_SelProc: Cannot Select.'
			,@val2 = ''
			,@val2str = '';
	END CATCH
		-- End of procedure  --   
END
GO

IF EXISTS (
		SELECT *
		FROM sysobjects
		WHERE type = 'P'
			AND NAME = 'USP_EIS_RPT_BR_VE_DQ_BeneActive_SelProc'
		)
BEGIN
	PRINT 'CREATED USP_EIS_RPT_BR_VE_DQ_BeneActive_SelProc';
END