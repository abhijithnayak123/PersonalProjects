CREATE PROCEDURE [dbo].[USP_IE_ImportGetGroupManagerCode] @GroupID INT
AS
BEGIN
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds    

	-- Body of procedure   
	SELECT DISTINCT ManagerCode AS BriefName
	FROM TBL_IE_STG_Estimate
	WHERE GroupID = @GroupID
		AND IsDeleted = 0
	ORDER BY ManagerCode;

	SET NOCOUNT OFF;
END
