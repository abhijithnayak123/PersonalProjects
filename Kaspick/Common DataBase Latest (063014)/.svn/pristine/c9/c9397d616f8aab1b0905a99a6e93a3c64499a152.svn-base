CREATE PROCEDURE [dbo].[USP_TR_GetAllEventType]
	-- paremeters here        
AS
BEGIN
	SELECT DISTINCT EventType
		,EventTypeID
		,RiskLevel
	FROM TBL_TR_EventTypeMaster
	ORDER BY EventType ASC
END
