CREATE PROCEDURE [dbo].[USP_EX_GetSysadminBenereportcommentsDetail]  
 -- paremeters here    
 @BRCommentID INT    
AS     
    
--  Variable Declarations  --    
Declare @procname    varchar(60);    
Declare @errmsg     varchar(1000);    
Declare @errnbr     int;    
    
-- Variables used for error handling     
--  Variable Data Assignment  --    
Set @procname = '[USP_EX_GetSysadminBenereportcommentsDetail]';    
    
-- Body of procedure  --    
    
SELECT  BenRptCmnt.BRCommentID,    
  BenRptCmnt.PortfolioCode,    
  BenRptCmnt.PORTFOLIOLABEL,    
  BenRptCmnt.PORTFOLIODESCRIPTION,    
  ModifiBenRptCmnt.ModifiedBy,    
  ModifiBenRptCmnt.ModifiedDate   
FROM TBL_INV_BeneReportComment BenRptCmnt INNER JOIN    
  TBL_INV_BeneReportComment ModifiBenRptCmnt ON BenRptCmnt.BRCOMMENTID = ModifiBenRptCmnt.BRCOMMENTID     
WHERE BenRptCmnt.BRCOMMENTID = @BRCommentID    
     
GO
IF EXISTS (SELECT
	*
FROM sysobjects
WHERE type = 'P'
AND name = 'USP_EX_GetSysadminBenereportcommentsDetail') 
BEGIN

PRINT 'CREATED USP_EX_GetSysadminBenereportcommentsDetail';
END
