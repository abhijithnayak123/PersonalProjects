SET NOCOUNT ON
-- TBL_KS_User migration script
SET IDENTITY_INSERT dbo.TBL_KS_User ON

INSERT INTO TBL_KS_User (
	UserID
	,FirstName
	,MiddleName
	,LastName
	,LoginName
	,Initials
	,IsActive
	,LastLoginDateTime
	,LoginCount
	,CreatedDate
	,CreatedUserID
	,ModifiedDate
	,ModifiedUserID
	,OfficeID
	,Phone
	,Email
	,MGRUserID
	,InnotrustContactID
	)
SELECT user_id
	,first_name
	,middle_name
	,last_name
	,login_name
	,initials
	,is_active
	,last_login_datetime
	,login_count
	,created_date
	,created_user_id
	,modified_date
	,modified_user_id
	,office_id
	,phone
	,email
	,mgr_user_id
	,Staff.ContactID
FROM $(ExcelsiorDB)..TBL_EIS_USER Usr
LEFT OUTER JOIN $(MappingDB)..TBL_KCOStaffLookup Staff on Usr.user_id = Staff.UserID

SET IDENTITY_INSERT dbo.TBL_KS_User OFF

