CREATE PROCEDURE [dbo].[USP_IE_ReviewGetSTGClientComment] @StagingID INT
AS
BEGIN
	SET NOCOUNT ON;
	SET LOCK_TIMEOUT 30000;-- 30 seconds    

	SELECT EP_ClientIEComment
		,ModifiedDate
	FROM TBL_IE_STG_Estimate
	WHERE StagingID = @StagingID

	SET NOCOUNT OFF;
END
