CREATE PROCEDURE USP_SIT_GetAllStatus
	-- paremeters here    
AS
BEGIN
	--  Initial Set statements  --    
	SET ANSI_NULLS OFF;
	SET QUOTED_IDENTIFIER OFF;

	--  Variable Declarations  --    
	--  Variable Data Assignment  --    
	-- Body of procedure  --  
	SELECT StatusName
		,SITStatusID
	FROM TBL_SIT_Status
	WHERE StatusName <> 'Null'
	ORDER BY DisplaySequence;

	SET NOCOUNT OFF;
END